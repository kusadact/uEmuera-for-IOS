;ふたなり細分化
@FUTANARI_CUSTOMIZE

PRINTFORML 
PRINTFORML 全部角色的扶他素質要怎麼辦？
PRINTFORML [1] 全部靠自己的想象
PRINTFORML [2] 全員細緻的設定
PRINTFORML [3] 無論什麼陰茎都沒問題（隨機付与素質）
INPUT
IF RESULT == 1
	FLAG:590 = 1
	RETURN
ELSEIF RESULT == 2
	LOCAL:1 = 0

	PRINTL 
	PRINTFORML 請設定竿的長度
	PRINTFORML [0] 很短
	PRINTFORML [1] 一般
	PRINTFORML [2] 稍長
	PRINTFORML [3] 很長
	PRINTFORML [4] 超長
	$INPUT_LOOP_00
	INPUT
	IF RESULT >= 0 && RESULT <= 4
		FLAG:570 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_00
	ENDIF
	
	PRINTL 請設定竿的粗細
	PRINTL [0] 就那樣
	PRINTL [1] 極粗
	$INPUT_LOOP_1
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:571 = RESULT
	ELSE
		GOTO INPUT_LOOP_1
	ENDIF

	PRINTFORML 請設定精力
	PRINTFORML [0] 薄弱
	PRINTFORML [1] 一般
	PRINTFORML [2] 精力超群
	PRINTFORML [3] 超精力超群
	$INPUT_LOOP_2
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:572 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_2
	ENDIF

	PRINTL 請設定包皮
	PRINTFORML [0] 一般
	PRINTFORML [1] 完全剥開
	PRINTFORML [2] 假性包茎
	PRINTFORML [3] 真性包茎
	$INPUT_LOOP_3
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:573 = RESULT
	ELSE
		GOTO INPUT_LOOP_3
	ENDIF

	PRINTL 請設定恥垢
	PRINTFORML [0] 清潔
	PRINTFORML [1] 恥垢体質
	$INPUT_LOOP_4
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:574 = RESULT
	ELSE
		GOTO INPUT_LOOP_4
	ENDIF

	PRINTL 請設定臭味
	PRINTFORML [0] 一般
	PRINTFORML [1] 悪臭
	$INPUT_LOOP_5
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:575 = RESULT
	ELSE
		GOTO INPUT_LOOP_5
	ENDIF

	;先天性後天性
	PRINTL 請設定是否為先天性
	PRINTFORML [0] 不設定
	PRINTFORML [1] 全員天生
	PRINTFORML [2] 全員後天的
	$INPUT_LOOP_6
	INPUT
	IF RESULT >= 0 && RESULT <= 2
		FLAG:576 = RESULT
	ELSE
		GOTO INPUT_LOOP_6
	ENDIF

	PRINTL 請設定竿的顔色
	PRINTFORML [0] 一般
	PRINTFORML [1] 黒光
	$INPUT_LOOP_7
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:577 = RESULT
	ELSE
		GOTO INPUT_LOOP_7
	ENDIF

	;次はTALENT:180からなので飛ばし

	PRINTL 請設定睾丸的有無
	PRINTFORML [0] 無睾丸
	PRINTFORML [1] 有睾丸
	PRINTFORML [2] 大睾丸
	PRINTFORML [3] 巨睾丸
	PRINTFORML [4] 超大睾丸
	$INPUT_LOOP_10
	INPUT
	IF RESULT >= 0 && RESULT <= 4
		FLAG:580 = RESULT
		IF RESULT > 0 && RESULT <= 4
			PRINTL 請設定睾丸毛
			PRINTFORML [0] 無毛
			PRINTFORML [1] 産毛
			PRINTFORML [2] 剛毛
			$INPUT_LOOP_11
			INPUT
			IF RESULT >= 0 && RESULT <= 2
				FLAG:581 = RESULT
			ELSE
				GOTO INPUT_LOOP_11
			ENDIF
		ENDIF
	ELSE
		GOTO INPUT_LOOP_10
	ENDIF
	

	

	;次はTALENT:185からなので飛ばし

	PRINTL 請設定精液的量
	PRINTFORML [0] 少量
	PRINTFORML [1] 一般
	PRINTFORML [2] 大量
	PRINTFORML [3] 超大量
	$INPUT_LOOP_15
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:585 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_15
	ENDIF

	PRINTL 請設定精液的濃度
	PRINTFORML [0] 薄弱
	PRINTFORML [1] 一般
	PRINTFORML [2] 特濃
	PRINTFORML [3] 超特濃
	$INPUT_LOOP_16
	INPUT
	IF RESULT >= 0 && RESULT <= 3
		FLAG:586 = RESULT - 1
	ELSE
		GOTO INPUT_LOOP_16
	ENDIF

	PRINTL 請設定精液的熱度
	PRINTFORML [0] 一般
	PRINTFORML [1] 很熱
	$INPUT_LOOP_17
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:587 = RESULT
	ELSE
		GOTO INPUT_LOOP_17
	ENDIF

	PRINTL 請設定精液的黄斑
	PRINTFORML [0] 白
	PRINTFORML [1] 有黄斑
	$INPUT_LOOP_18
	INPUT
	IF RESULT >= 0 && RESULT <= 1
		FLAG:588 = RESULT
	ELSE
		GOTO INPUT_LOOP_18
	ENDIF

	;ランダム発生をOFFにする
	FLAG:590 = 0

	;別のところからも飛ばしたいので分ける
	CALL FUTANARI_CUSTOMIZE_GET

ELSEIF RESULT == 3
	CALL FUTANARI_RANDOM_GET
	FLAG:590 = 2
ENDIF

;全キャラ同時入手用
@FUTANARI_CUSTOMIZE_GET

FOR LOCAL:0, 1, CHARANUM
	FOR LOCAL:1,0,20
		TALENT:(LOCAL:0):(170 + LOCAL:1) = FLAG:(570 + LOCAL:1)
		CFLAG:(LOCAL:0):515 = 1
	NEXT
NEXT

;各キャラ個別入手用
@FUTANARI_CUSTOMIZE_GET_2(ARG:0)
SIF CFLAG:(ARG:0):515 == 1
	RETURN
FOR LOCAL:1, 1, 20
	TALENT:(ARG:0):(170 + LOCAL:1) = FLAG:(570 + LOCAL:1)
NEXT
CFLAG:(ARG:0):515 = 1

;全キャラ同時ランダム素質入手用
@FUTANARI_RANDOM_GET

FOR LOCAL:0, 1, CHARANUM
	;数値範囲がバラバラなので1個ずつ
	TALENT:(LOCAL:0):170 = RAND:5 - 1
	TALENT:(LOCAL:0):171 = RAND:2
	TALENT:(LOCAL:0):172 = RAND:4 - 1
	TALENT:(LOCAL:0):173 = RAND:4
	TALENT:(LOCAL:0):174 = RAND:2
	TALENT:(LOCAL:0):175 = RAND:2
	;先天的か後天的かだけふたなり素質が必要
	IF TALENT:(LOCAL:0):121
		TALENT:(LOCAL:0):176 = RAND:2
	ENDIF
	TALENT:(LOCAL:0):177 = RAND:2
	TALENT:(LOCAL:0):180 = RAND:5
	IF TALENT:(LOCAL:0):180
		TALENT:(LOCAL:0):181 = RAND:3
	ENDIF
	TALENT:(LOCAL:0):185 = RAND:4 - 1
	TALENT:(LOCAL:0):186 = RAND:4 - 1
	TALENT:(LOCAL:0):187 = RAND:2
	TALENT:(LOCAL:0):188 = RAND:2

	CFLAG:(LOCAL:0):515 = 1
NEXT

;各キャラ個別ランダム素質入手用
@FUTANARI_RANDOM_GET_2(ARG:0)
SIF CFLAG:(ARG:0):515 == 1
	RETURN
SIF TALENT:(ARG:0):122
	RETURN
;数値範囲がバラバラなので1個ずつ
TALENT:(ARG:0):170 = RAND:5 - 1
TALENT:(ARG:0):171 = RAND:2
TALENT:(ARG:0):172 = RAND:4 - 1
TALENT:(ARG:0):173 = RAND:4
TALENT:(ARG:0):174 = RAND:2
TALENT:(ARG:0):175 = RAND:2
;先天的か後天的かだけふたなり素質が必要
IF GET_PENIS(ARG:0) > 1
	TALENT:(ARG:0):176 = RAND:2
ENDIF
TALENT:(ARG:0):177 = RAND:2
TALENT:(ARG:0):180 = RAND:5
IF TALENT:(ARG:0):180
	TALENT:(ARG:0):181 = RAND:3
ENDIF
TALENT:(ARG:0):185 = RAND:4 - 1
TALENT:(ARG:0):186 = RAND:4 - 1
TALENT:(ARG:0):187 = RAND:2
TALENT:(ARG:0):188 = RAND:2

CFLAG:(ARG:0):515 = 1

