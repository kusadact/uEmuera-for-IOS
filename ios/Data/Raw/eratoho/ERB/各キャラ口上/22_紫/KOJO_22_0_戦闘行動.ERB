					ENDIF
				ELSE
					IF FLAG:AUTOMODE
						PRINTFORML 「この体でも、結界を展開するくらいは造作も無いわね」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「この体でも、結界を展開するくらいは造作も無いわね」
					ENDIF
				ENDIF
			;リロード
			ELSEIF ARG:2 == 1
				IF CFLAG:805 == 5
					CFLAG:805 = 0
				ELSE
					IF FLAG:AUTOMODE
						PRINTFORML 「頼らざるを得ないわね」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「頼らざるを得ないわね」
					ENDIF
					CFLAG:805 = 5
				ENDIF
			ENDIF
;		;アイテム
;		CASE 2
		;前進
		CASE 3
			;発情時・MP1/4時・HP0時・妊娠時・朦朧時で他に味方がいる
			IF FLAG:10 && (CFLAG:28 || MAXBASE:MP/4 >= BASE:MP || BASE:HP <= 0 || CFLAG:36 >= 3)
