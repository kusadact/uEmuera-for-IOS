	IF RAND:3 == 0
		IF FLAG:AUTOMODE
			PRINTFORML 「ぁぐっ……」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「ぁぐっ……」
		ENDIF
	ELSEIF RAND:2 == 0
		IF FLAG:AUTOMODE
			PRINTFORML 「……やん……」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「……やん……」
		ENDIF
	ELSE
		IF FLAG:AUTOMODE
			PRINTFORML 「…………は、んっ」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「…………は、んっ」
		ENDIF
	ENDIF
	RESETCOLOR
	RETURN
ENDIF

