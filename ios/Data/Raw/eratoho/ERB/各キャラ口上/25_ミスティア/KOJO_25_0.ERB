		IF FLAG:AUTOMODE
			PRINTFORML 「ゆ、幽々子さん、私は食べられないですぅ……」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「ゆ、幽々子さん、私は食べられないですぅ……」
		ENDIF
	ELSEIF RAND:2 == 0
		IF FLAG:AUTOMODE
			PRINTFORML 「くぅ……くぅ……」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「くぅ……くぅ……」
		ENDIF
	ELSE
		IF FLAG:AUTOMODE
			PRINTFORML 「ねんねして……ころっと……ＺＺＺ」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「ねんねして……ころっと……ＺＺＺ」
		ENDIF
	ENDIF
	RESETCOLOR
	;口上記入済み
	RETURN 1
ENDIF

SELECTCASE ARG:0
