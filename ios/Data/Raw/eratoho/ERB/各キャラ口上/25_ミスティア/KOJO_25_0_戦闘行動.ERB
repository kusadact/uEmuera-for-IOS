					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「間近で聞いてよ、私の歌♪」
				ENDIF
			ENDIF
		;後退
		CASE 4
			;発情時・MP1/4時・HP0時・妊娠時・朦朧時
			IF CFLAG:28 || MAXBASE:MP/4 >= BASE:MP || BASE:HP <= 0 || CFLAG:36 >= 3
				IF FLAG:AUTOMODE
					PRINTFORML 「うぅ……」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「うぅ……」
				ENDIF
			ELSE
				IF FLAG:AUTOMODE
					PRINTFORML 「ここからでも十分声は届くわね～」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「ここからでも十分声は届くわね～」
				ENDIF
			ENDIF
		;逃走
		CASE 5
	ENDSELECT
