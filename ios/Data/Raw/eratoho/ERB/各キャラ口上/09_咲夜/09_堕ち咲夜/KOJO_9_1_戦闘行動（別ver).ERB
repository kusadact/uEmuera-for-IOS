	SELECTCASE ARG:1
		;平常行動
		CASE 71
			IF FLAG:AUTOMODE
				PRINTFORML 「えっ!?何があったの？」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「えっ!?何があったの？」
			ENDIF
		;72　催眠自慰は46　強制自慰と共用
			IF FLAG:AUTOMODE
				PRINTFORML 「あぁ……早くシなきゃ」
