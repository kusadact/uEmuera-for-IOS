		TFLAG:57 = 40 + ((EXP:2+EXP:14) / 900)
		IF TFLAG:57 > 650
			TFLAG:57 = 650
		ENDIF
		TFLAG:58 = 140
		
	;パイズリ
	CASE 86
		;ベース威力は話術＆Ｂ感覚＆精液経験経験依存（自然に上がる安定した強さ）
		TFLAG:57 = 50 + ((EXP:2+EXP:7+EXP:14) / 1200)
		IF TFLAG:57 > 450
			TFLAG:57 = 450
		ENDIF
		TFLAG:58 = 150
		
	ENDSELECT


