@BTL_END_START_7_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
;戦闘開始（いや、ファイル名がアレなのは解ってるから後で直すかも）

	;戦意喪失
	IF CFLAG:29
		RETURN
	ENDIF

	SELECTCASE TFLAG:14
	;味方先制
	CASE 1
		IF FLAG:AUTOMODE
			PRINTFORML 「気づかれてない…今なら安全に逃げられますね」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「気づかれてない…今なら安全に逃げられますね」
		ENDIF
		
	;敵先制
	CASE 2
		IF FLAG:AUTOMODE
			PRINTFORML 「…………えっ！？　敵…っ！！！」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「…………えっ！？　敵…っ！！！」
		ENDIF
	
	ENDSELECT

RETURN

