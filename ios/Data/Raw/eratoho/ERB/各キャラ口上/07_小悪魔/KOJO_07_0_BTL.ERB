@BTL_ETC_FIRST_ENCOUNT_7_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
;ARG:1 の種族と初遭遇

	;戦意喪失or寝てる時に会った
	IF CFLAG:29 || TFLAG:14 == 3
		RETURN
	ENDIF

	SELECTCASE ARG:1
	;クイーン
	CASE 1
		IF FLAG:AUTOMODE
			PRINTFORML 「この押し潰される様な雰囲気…今の私%TACHI()%で勝てるかどうか…」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「この押し潰される様な雰囲気…今の私%TACHI()%で勝てるかどうか…」
		ENDIF
	;ガーディアン
	CASE 3
		CALL GET_PT_CHAR_NO_7_0(8, 1)
		;パーティにパチュリーが健在
		IF RESULT
			IF FLAG:AUTOMODE
				PRINTFORML 「あ、パチュリー様！　見て下さい、出口らしきものが！」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「あ、パチュリー様！　見て下さい、出口らしきものが！」
			ENDIF
		ELSE
			IF FLAG:AUTOMODE
				PRINTFORML 「あの光は…出口？」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「あの光は…出口？」
			ENDIF
		ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 「…でもどうやら、ここから出るにはあのゴツいのを倒さないといけない様ですね」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「…でもどうやら、ここから出るにはあのゴツいのを倒さないといけない様ですね」
			ENDIF
	;バイオプラント
	CASE 4
		IF FLAG:AUTOMODE
			PRINTFORML 「あれは…この辺りの怪物を産み出しているんですね。なんとかして止める必要がありそうです。」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「あれは…この辺りの怪物を産み出しているんですね。なんとかして止める必要がありそうです。」
		ENDIF
	
	;サキュバス
	CASE 41
		IF FLAG:AUTOMODE
			PRINTFORML 「戦う気ですか…わかりました。どちらの技が優れているか、決着を付けましょうか」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「戦う気ですか…わかりました。どちらの技が優れているか、決着を付けましょうか」
		ENDIF
	
	;小悪魔
	CASE 70
		IF FLAG:AUTOMODE
			PRINTFORML 「はぁ……ここの空気に染まってすっかり理性を失っちゃってる様ですね……」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「はぁ……ここの空気に染まってすっかり理性を失っちゃってる様ですね……」
		ENDIF
		IF FLAG:AUTOMODE
			PRINTFORML 「ちょっと痛いかもしれませんけど…我慢して下さい！」
			TWAIT FLAG:AUTOMODE * 400, 0
		ELSE
			PRINTFORMW 「ちょっと痛いかもしれませんけど…我慢して下さい！」
		ENDIF
	
	ENDSELECT

RETURN

