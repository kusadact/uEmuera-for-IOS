;イベント

@EVENT_RANDOM_7_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
;ランダムイベント

	SELECTCASE ARG:0
	;トラップ
	CASE 200
		CALL EVENT_RANDOM_TRAP_7_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
			
	;PT分断テレポート
	CASE 201
		SELECTCASE ARG:1
		;飛ばされる前
		CASE 0
			IF FLAG:AUTOMODE
				PRINTFORML 「きゃっ！？　眩しい─────────……」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「きゃっ！？　眩しい─────────……」
			ENDIF
		;飛ばされた後
		CASE 1
			IF FLAG:AUTOMODE
				PRINTFORML 「……ん…ここは……どこかに飛ばされちゃったみたいですね…」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「……ん…ここは……どこかに飛ばされちゃったみたいですね…」
			ENDIF
			CALL GET_PT_CHAR_NO_7_0(8)
			;パチュリーが居る
			IF RESULT > 0
				IF FLAG:AUTOMODE
					PRINTFORML 「あれ…%CALLNAME_7_0(RESULT)%？　%CALLNAME_7_0(RESULT)%！！！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「あれ…%CALLNAME_7_0(RESULT)%？　%CALLNAME_7_0(RESULT)%！！！」
				ENDIF
				IF FLAG:AUTOMODE
					PRINTFORML 「近くには居ないですか…早く%CALLNAME_7_0(RESULT)%を探さないと…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「近くには居ないですか…早く%CALLNAME_7_0(RESULT)%を探さないと…」
				ENDIF
			
			ELSEIF FLAG:10 && FLAG:11
				IF FLAG:AUTOMODE
					PRINTFORML 「皆さんの姿も見えない…さてどうしましょうか…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「皆さんの姿も見えない…さてどうしましょうか…」
				ENDIF
			ELSEIF FLAG:10
				IF FLAG:AUTOMODE
					PRINTFORML 「あ…%CALLNAME_7_0(FLAG:10)%は……姿が見えないですね。さてどうしましょうか…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「あ…%CALLNAME_7_0(FLAG:10)%は……姿が見えないですね。さてどうしましょうか…」
				ENDIF
			ENDIF
		ENDSELECT
		
			
	ENDSELECT

RETURN


