
; ------------------------------------------------------------------------------
; ■ 15 敵の拘束攻撃 (命中)
; ------------------------------------------------------------------------------

; □ 汎用口上
@KOJO_27_0_SYSTEM_MESSAGE_15(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 睡眠
	IF CFLAG:25
		SELECTCASE KOJO_MODE
		CASE "強気"
			CALL KOJO_27_0_ADD_MESSAGE, @"「…うう…っん！」\n"
			CALL KOJO_27_0_PARAGRAPH_MESSAGE
			CALL KOJO_27_0_ADD_MESSAGE, @"\d睡眠を妨害される不快感に、%CALLNAME:TARGET%は眠ったまま身をよじっている\d\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"\d睡眠中の%CALLNAME:TARGET%は、抵抗することもなく%ARGS:1%に捕らえられた\d\n"
		ENDSELECT
		RETURN 1
	ENDIF
	
	; 戦意喪失
	IF CFLAG:29
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「おいしく…　たべてね…♥」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「や、やだ…　離して……」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ああ…　つかまっちゃ…　た……」\n"
		ENDSELECT
		RETURN 1
	ENDIF
	
	; 諦めた
	IF TFLAG:16 == 2
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「無抵抗なうさぎさんは、こうやって食べられちゃうのね♥」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"（やだ…　怖い……！）\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「うぁ…　あぁ…っ……」\n"
			CALL KOJO_27_0_PARAGRAPH_MESSAGE
			CALL KOJO_27_0_ADD_MESSAGE, @"\d足がすくみ動けなくなった%CALLNAME:TARGET%は、あっさりと%ARGS:1%に捕らえられた\d\n"
		ENDSELECT
		RETURN 1
	ENDIF
	
	; 催眠中
	IF KOJO_27_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			; 催眠深度によって変化
			SELECTCASE CFLAG:43
			CASE 1
				CALL KOJO_27_0_ADD_MESSAGE, @"（や…やだぁっ　カラダ…いうこときかない！？）\n"
				CALL KOJO_27_0_ADD_MESSAGE, @"（これじゃあ、あたしがスキモノみたいじゃない！）\n"
			CASE 2
				CALL KOJO_27_0_ADD_MESSAGE, @"（あれ？　あたし何を嫌がってたんだっけ？）\n"
				CALL KOJO_27_0_ADD_MESSAGE, @"（今から犯してもらうんだから…　ちゃんと言うこときかなきゃ…）\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「はい…よろしくお願いします…」\n"
				CALL KOJO_27_0_ADD_MESSAGE, @"「いっぱい…気持よくしてください…」\n"
			ENDSELECT
			RETURN 1
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_27_0_ADD_MESSAGE, @"「捕まえるのはともかく、酷い格好させるのねえ♥」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「やぁん♥　食べられちゃう♥」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あんっ♥　えっちな捕まえかた……ぁ♥」\n"
	CASE "発情"
		CALL KOJO_27_0_ADD_MESSAGE, @"（なんでっ！？　身体にうまく……　力入んない……っ！？）\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「だめぇっ！　今はだめだってぇっ！\n　身体あつくて……っ　ヘンになってるからぁ　ホントにだめなのぉっ！」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（やだ……　あたし……期待してる……）\n"
	CASE "強気"
		CALL KOJO_27_0_ADD_MESSAGE, @"「うぐっ！　動けない！」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「もうっ！　離してよ！」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「このぉ……はなせ～！」\n"
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「や……だっ……　こんな格好……っ！？」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「うわぁっ！？」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「やっば……！？」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ちょっ……！？　ちょっとまってー！？」\n"
	ENDSELECT
	CALL KOJO_27_0_FIX_MESSAGE
	
	; 敵の先制攻撃なら追加口上
	IF CFLAG:24 && KEEP_TURN == 1
		SELECTCASE KOJO_MODE
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"「そんな！？　急にっ！？」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ひぃっ……　逃げられないっ……」\n"
		CASE "強気"
			CALL KOJO_27_0_ADD_MESSAGE, @"「不意打ち拘束とか、卑怯っ！！」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「不意打ちでこの攻撃はちょっとズルくないかしら～！？」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ちょっと待って！？　こっちはまだ戦う準備できてない…のにっ！？」\n"
		ENDSELECT
	ENDIF
	CALL KOJO_27_0_FIX_MESSAGE
	
	; この戦闘中に拘束された事があるなら追加口上
	IF GETBIT(CFLAG:900, 4)
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ああん♥　また捕まっちゃったぁ♥」\n"
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"「嫌あぁぁっ！　またっ……　また捕まったぁ……っ」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「ウソでしょ！？　このあたしが何度も捕まるなんて！？」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ううっ……　一度は逃げ出せたのに、また捕まってぇ……」\n"
		ENDSELECT
	ENDIF
	CALL KOJO_27_0_FIX_MESSAGE
	
	; 服を破って拘束を回避失敗時追加口上
	IF BASE:3 != PREV_BASE:3
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「自分から脱いで捕まっちゃうなんて、誘ってるみたい♥」\n"
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"「う、うそ…っ…　いやああぁ…っ…！」\n\d服を破いた上に拘束からも逃れられなかった%CALLNAME:TARGET%は、自分の取った悪手に顔を青ざめさせた\d\n"
		CASE "強気"
			CALL KOJO_27_0_ADD_MESSAGE, @"「破いた服だけで満足しなさいよ！」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「ううっ…　服の破き損だわ…」\n"
		ENDSELECT
	ENDIF
	CALL KOJO_27_0_FIX_MESSAGE
	
	; 選択コマンド別追加口上
	SELECTCASE CFLAG:50
	; 発情抱きつき
	CASE 96
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「いいトコで邪魔したんだから、責任とってよね♥」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「うう……　いい所だったのにぃ……」\n"
		ENDSELECT
		CALL KOJO_27_0_PARAGRAPH_MESSAGE
		CALL KOJO_27_0_ADD_MESSAGE, @"\d%ARGS:1%に無理矢理引き剥がされた%CALLNAME:TARGET%は、残念そうに%CALLNAME:(CFLAG:51)%を見ている…\d\n"
	ENDSELECT
	CALL KOJO_27_0_FIX_MESSAGE
	
RETURN 1


