; □ 汎用口上
@KOJO_27_0_SYSTEM_MESSAGE_61(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM LCOUNT
#DIM ENEMY_ID

	; 状態異常で喋れない場合は何もしないで帰る
	SIF CFLAG:25 || CFLAG:29
		RETURN 0
		
	SELECTCASE KOJO_MODE
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「ろくでもない生き物ばっか出てくる……」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「まーた変なのが出てきた……」\n"
	ENDSELECT
	
	; 触手種別による汎用口上
	; 呼び出された敵情報を確定するため敵IDを取得
	; りばーすではDAの敵情報が足りないので未適用
	IF !KOJO_27_0_IS_REVERSE()
		FOR LCOUNT, 1, VARSIZE("DA", 1)
			IF ARG:1 == DA:LCOUNT:0
				ENEMY_ID = LCOUNT
				BREAK
			ENDIF
		NEXT
		
		; 女性系
		IF GETBIT(DA:ENEMY_ID:85, 0)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「この場所に取り込まれたか、元々ああいう生き物か\n　どっちにしろろくな相手じゃないのは確かね」\n"
			ENDSELECT
		ENDIF
		
		; 植物系
		IF GETBIT(DA:ENEMY_ID:85, 1)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「よく動く植物だこと……」\n"
			ENDSELECT
		ENDIF
		
		; 動物系
		IF GETBIT(DA:ENEMY_ID:85, 2)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「絶対こんな場所で生きてる動物に、まともなのは居ないし」\n"
			ENDSELECT
		ENDIF
		
		; 丸呑系
		IF GETBIT(DA:ENEMY_ID:85, 3)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「うげぇ……　絶対アレ相手を溶かして食べちゃうタイプの奴だ……」\n"
			ENDSELECT
		ENDIF
		
		; 海産系
		IF GETBIT(DA:ENEMY_ID:85, 4)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「なんでこんな場所に海の生き物がいるのよ～」\n"
			ENDSELECT
		ENDIF
		
		; ローパー系
		IF GETBIT(DA:ENEMY_ID:85, 5)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「うねうねしてて近付きたくないなあ」\n"
			ENDSELECT
		ENDIF
		
		; スライム系
		IF GETBIT(DA:ENEMY_ID:85, 6)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「スライム系って何やってくるか判んないから怖いのよー」\n"
			ENDSELECT
		ENDIF
		
		; 男性系
		IF GETBIT(DA:ENEMY_ID:85, 7)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「メスと見れば見境なしって感じね……」\n"
			ENDSELECT
		ENDIF
		
		; 罠系
		IF GETBIT(DA:ENEMY_ID:85, 8)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「あたしは罠に掛けるのが好きなの、掛かるのは嫌いよ！」\n"
			ENDSELECT
		ENDIF
		
		; 機械系
		IF GETBIT(DA:ENEMY_ID:85, 9)
			SELECTCASE KOJO_MODE
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「はあぁ!?　何でこんなのが動いてるワケ!?」\n"
			ENDSELECT
		ENDIF
	ENDIF
RETURN 1


