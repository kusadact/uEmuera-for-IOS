
; ------------------------------------------------------------------------------
; ■ 200 ランダムイベント発生 - 137 壁触手
; ------------------------------------------------------------------------------

; □ 1 回避失敗
@KOJO_27_0_SYSTEM_MESSAGE_200_137_1(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ああん♥　捕まっちゃったぁ♥」\n"
	CASE "発情"
		CALL KOJO_27_0_ADD_MESSAGE, @"「いやああぁぁっ！　そんな急にっ！？」\n"
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「かっ、壁からぁっ！？　そんなの聞いてないわよぉ！？」\n"
	ENDSELECT
RETURN 1


; □ 2 回避成功
@KOJO_27_0_SYSTEM_MESSAGE_200_137_2(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	SELECTCASE KOJO_MODE
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「タイミングが甘いわね、罠ってのはもうちょい引きつけてから」\n"
	ENDSELECT
RETURN 1
