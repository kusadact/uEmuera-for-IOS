
; ------------------------------------------------------------------------------
; ■ 14 敵の性攻撃 (命中) - 攻撃種類別 0353 スライム 全体A
; ------------------------------------------------------------------------------

@KOJO_27_0_SYSTEM_MESSAGE_14_353(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 追加地の文関数(前)を登録
	CALL KOJO_27_0_REGIST_DESCRIPTION_BEFORE("KOJO_27_0_SYSTEM_MESSAGE_14_DESCRIPTION_BEFORE")

	; 追加地の文関数(後)を登録
	CALL KOJO_27_0_REGIST_DESCRIPTION_AFTER("KOJO_27_0_SYSTEM_MESSAGE_14_DESCRIPTION_AFTER")
	
	; 睡眠・喪失・諦め
	SIF CFLAG:25 || CFLAG:29 || TFLAG:16 == 2
		RETURN 0

	; 催眠中
	IF KOJO_27_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ああっ♥　おしりの穴に、あたたかいヌルヌルがっ♥」\n"
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「ひぃっ！？　おしりの中で、ヌメヌメしたのが動いてるっ！？」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「こ、こいつ！　押さえても指の隙間から這い上がってぇ……っ！？」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「うわわっ！？　足下から登ってくるなぁっ！　気持ち悪いっ！」\n"
	ENDSELECT

	CALL KOJO_27_0_PARAGRAPH_MESSAGE
	
	; 地の文側で\nするので口上側は\nで終わること
RETURN 1
