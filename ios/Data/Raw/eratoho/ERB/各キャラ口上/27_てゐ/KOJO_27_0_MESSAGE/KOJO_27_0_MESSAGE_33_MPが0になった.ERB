			CALL KOJO_27_0_ADD_MESSAGE, @"（目が覚めたら……また、ご主人様に……♥♥）\n"
		ENDIF

		RETURN 1
		
	; ペットへ身体を捧げた
	ELSEIF TEMP_FLAG:1
		; 対ペットで喪失はレアケースなので優先口上指定
		CALL KOJO_27_0_SET_IMPORTANT_FLAG()
		
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ペットに犯されて……っ♥　イキ負けるなんてぇ……♥♥」\n"
