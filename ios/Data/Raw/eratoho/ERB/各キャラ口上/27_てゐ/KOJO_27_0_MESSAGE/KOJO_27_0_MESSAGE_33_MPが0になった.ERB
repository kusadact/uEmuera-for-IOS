		CASE "強気"
			CALL KOJO_27_0_ADD_MESSAGE, @"（飼い犬に手を噛まれる……か……　不覚……）\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"（こんな奴、飼うんじゃなかった……）\n"
		ENDSELECT
		
	; 通常時
	ELSE
		; 催眠中
		IF KOJO_27_0_IS_HYPNO()
			; 催眠種類で分岐
			SELECTCASE FLAG:526
			; 拘束・肉体操作系
			CASE 0
				
			; 発情・感情操作系
			CASE 1
				
			; 無意識・認識操作系
			CASE 2
				; 催眠深度によって変化
				SELECTCASE CFLAG:43
				CASE 1
					CALL KOJO_27_0_ADD_MESSAGE, @"（こんな所で倒れたら……　頭の中……完全に、壊されちゃう……）\n"
				CASE 2
					CALL KOJO_27_0_ADD_MESSAGE, @"（ああ……ぁ……　もっと……お便所交尾しなきゃ……ダメ……なのにぃ……）\n"
				CASEELSE
					CALL KOJO_27_0_ADD_MESSAGE, @"（もう……動けない……　せめて、オナホールとして……使ってほし……い……な……）\n"
					CALL KOJO_27_0_ADD_MESSAGE, @"（ごめん……なさい……　おトイレ……故障しちゃい……ました……）\n"
				ENDSELECT
				RETURN 1
			ENDSELECT
		ENDIF

		; 通常時
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「もうだめぇ♥　あたまのなか……トロットロに……　とけちゃっ……たぁ……♥♥」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「もうだめぇ♥　こうさんっ♥　こうさんしますぅ…っ♥♥」\n"
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"（もう逆らえないんだ……　あたし……）\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"（やだ……　身体に力、はいんないよぉ……）\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"（やば……　あたま……　ぼやけて……）\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"（あたしの幸せ……　どこ……いっちゃったんだろ……）\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"（だめ……　もう、動けな……い……）\n"
		ENDSELECT
		
		; まだ行動中の味方がいるなら追加口上
		IF (TARGET != 1 && CHECK_FINE(1)) || (FLAG:10 && TARGET != FLAG:10 && CHECK_FINE(FLAG:10)) || (FLAG:11 && TARGET != FLAG:11 && CHECK_FINE(FLAG:11))
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"（あたしがエッチな囮やるから、あとは頑張ってねぇ……♥）\n"
				CALL KOJO_27_0_ADD_MESSAGE, @"（他のヒトが頑張ってるから、あたしは気持ちいいことされてれば……いいよね……♥）\n"
			CASE "発情"
				CALL KOJO_27_0_ADD_MESSAGE, @"（あたしはもう……ダメ……　後は、よろしく……）\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"（後は……　任せたわ……）\n"
			ENDSELECT
		ENDIF
	ENDIF
RETURN 1


