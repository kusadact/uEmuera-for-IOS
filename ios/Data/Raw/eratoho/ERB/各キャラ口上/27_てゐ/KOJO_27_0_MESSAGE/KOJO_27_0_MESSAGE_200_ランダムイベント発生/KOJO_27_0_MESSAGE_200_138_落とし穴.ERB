
; ------------------------------------------------------------------------------
; ■ 200 ランダムイベント発生 - 138 落とし穴
; ------------------------------------------------------------------------------

; □ 1 回避失敗
@KOJO_27_0_SYSTEM_MESSAGE_200_138_1(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ああっ♥　ナカ…っ、ぬるぬるがいっぱいっ♥♥」\n"
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「あたしは落とし穴に落とす方なのにいぃっ！？」\n"
	ENDSELECT
RETURN 1


; □ 2 回避成功
@KOJO_27_0_SYSTEM_MESSAGE_200_138_2(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_27_0_ADD_MESSAGE, @"「つい避けちゃったけど、ちょっと落ちてみたいかも……」\n"
	CASE "強気"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あたしを落とし穴に落とそうなんて百年早いわ！」\n"
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「うっわ～、エグい落とし穴作るもんね～」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（うわ…あんなトコ落ちたら…あたし…）\n"
	ENDSELECT
RETURN 1


; □ 3 回避失敗 & 寄生
@KOJO_27_0_SYSTEM_MESSAGE_200_138_3(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
