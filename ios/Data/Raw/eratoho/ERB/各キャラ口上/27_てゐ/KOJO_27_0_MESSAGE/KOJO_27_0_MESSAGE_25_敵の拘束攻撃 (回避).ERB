
; ------------------------------------------------------------------------------
; ■ 25 敵の拘束攻撃 (回避)
; ------------------------------------------------------------------------------

; □ 汎用口上
@KOJO_27_0_SYSTEM_MESSAGE_25(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
#DIM PERCENT

	; 催眠中
	IF KOJO_27_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"「身体の主導権取られちゃったし、捕まるのはしょうがないと思ってたのにぃ」\n"
			CASE "発情"
				CALL KOJO_27_0_ADD_MESSAGE, @"「う、動けない所を捕まえるんじゃないの？」\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「動けなくなった相手は捕まえるまでもないって事？」\n"
				CALL KOJO_27_0_ADD_MESSAGE, @"「その攻撃外す意味わかんないんだけど？　相手の動きを止めてから捕まえるのって必勝でしょ」\n"
			ENDSELECT
			RETURN 1
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_27_0_ADD_MESSAGE, @"「捕まえたいなら、もっとねっとりと絡みつかなきゃね♪」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「その程度のテクじゃあたしを捕まえるなんて無理無理♪」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あたしを捕まえたいならもっとがんばって♥」\n"
	CASE "発情"
		CALL KOJO_27_0_ADD_MESSAGE, @"「よ、避けられた……」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あ、危なかった……」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あんなのに捕まったら……」\n"
		CALL KOJO_27_0_PARAGRAPH_MESSAGE
		CALL KOJO_27_0_ADD_MESSAGE, @"\d%ARGS:1%の攻撃を回避できた事に、%CALLNAME:TARGET%は火照る身体を抱きしめながら安堵の声を漏らした…\d\n"
	CASE "強気"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ふふん♪　そんな大振りじゃあたしを捕まえるなんて無理よ～♪」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あたしを捕まえようなんて数万年早いわよ！」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ウサギ一匹捕まえられないとか、才能ないんじゃなーい？」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「遅い♪　遅い、おそ～い♪」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ふっふ～ん♪　華麗に回避♪」\n"
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「残念でした、逃げるのは得意なの」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あっぶな……！」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「ふぅ、危ない危ない……」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あんなのに捕まるとか、ぞっとしないわ……」\n"
	ENDSELECT
	CALL KOJO_27_0_FIX_MESSAGE
	
	; 速度低下中 追加口上
	IF CFLAG:300 > 0
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"（こっちの動きは阻害されてるし、そのうち捕まっちゃうわね♥）\n"
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"（こっちの動き鈍ってるし、次は避けられるかどうか……）\n"
		CASE "強気"
			CALL KOJO_27_0_ADD_MESSAGE, @"「こっちの動きを縛っておいて、まだ当てられないの？」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「ふぅ、やっぱ動きにくいと避けるのは難しいわ」\n"
		ENDSELECT
	ENDIF
	CALL KOJO_27_0_FIX_MESSAGE
	
	; この戦闘中に拘束された事があるなら追加口上
	IF GETBIT(CFLAG:900, 4)
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ほらほら、もっと頑張って♪　逃げたウサギさんはここですよ～♪」\n"
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"（一度は捕まったんだから、ちゃんと警戒しないと……！）\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「そう何度も捕まってたまるもんですか！」\n"
		ENDSELECT
	ENDIF
	CALL KOJO_27_0_FIX_MESSAGE
	
	; 服を破って拘束を回避時追加口上
	IF BASE:CP != PREV_BASE:3
		SELECTCASE KOJO_MODE
		CASE "淫乱"
			CALL KOJO_27_0_ADD_MESSAGE, @"「捕まえる前に服を破るのって、えっちだぁ♥」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「捕まえるフリをして、服を破るのが目的かしら？　やらし～んだぁ♪」\n"
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"「服がなければ今頃……」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「た、助かったけど……　服が……」\n"
		CASE "強気"
			CALL KOJO_27_0_ADD_MESSAGE, @"「もう！　服は貴重品なのに！」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「破れた服の分、痛い目にあってもらいましょうか！」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「あたしの服は高いわよ？」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「服がなければ危なかったわ……」\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「ああもう破けちゃったじゃない……　捕まるよりマシかな……」\n"
		ENDSELECT
		
		; 破られる前の服破損状態で追加口上
		PERCENT = PREV_BASE:3 * 100 / MAXBASE:CP
		
		; 新品
		IF PERCENT >= 100
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"「綺麗な服ダメにされるのって、興奮するかも♥」\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「ああっ、新品の服がぁ」\n"
				CALL KOJO_27_0_ADD_MESSAGE, @"「まともな服着てたから避けられたって考えるべきね」\n"
			ENDSELECT
			
		; 通常
		ELSEIF PERCENT > 66
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"「チラチラ肌が見えるのってエッチよね～♪」\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「ちょっと破れてたけど、いい服だったのに～」\n"
			ENDSELECT
			
		; 損傷
		ELSEIF PERCENT > 33
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"「どんどん隠すのが難しくなってく～♥」\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「元々ボロい服だし、これ以上破れたって気にしないわ」\n"
			ENDSELECT
			
		; 中破
		ELSE
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"「ああん♥　ほとんど裸ぁ♥」\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「ボロ布みたいな服だったし、アレで攻撃避けられるならめっけもんかな」\n"
			ENDSELECT
		ENDIF
	ENDIF
RETURN 1


