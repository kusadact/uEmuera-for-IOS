			CALL KOJO_27_0_ADD_MESSAGE, @"（あたし…おねだりしてる…っ…　誰も見てないからって、ガマンできなくなってる…）\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"（誰も見てないから…　ちょっとだけ…いいよね……）\n"
		ENDSELECT
	ENDIF
	
	; 先制攻撃成功時 追加口上
	IF KOJO_27_0_GET_PREV_ARG(0) == 60 && KOJO_27_0_GET_PREV_ARG(2) == 1
		SELECTCASE KOJO_MODE
