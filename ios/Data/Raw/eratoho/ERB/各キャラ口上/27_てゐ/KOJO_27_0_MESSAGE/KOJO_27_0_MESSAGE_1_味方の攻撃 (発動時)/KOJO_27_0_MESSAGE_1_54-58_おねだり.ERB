; XX おねだり
; ◆ 前回情報参照
; ◇ 200 ランダムイベント発生
; ● 179 結婚式場
; ○ 3 誓いの公開レイプ
@KOJO_27_0_SYSTEM_MESSAGE_1_58_PREV_200_179_3(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 優先口上指定
	CALL KOJO_27_0_SET_IMPORTANT_FLAG()
	
	; 追加地の文関数を登録
	; 無印触手宮では口上表示後の直接描画、りばーす東方触手宮ではPRINT_MESSAGEで処理される
	; 表示タイミングが違うので、会わせた関数を別々に登録する
	IF KOJO_27_0_IS_REVERSE()
		CALL KOJO_27_0_REGIST_DESCRIPTION_BEFORE("KOJO_27_0_SYSTEM_MESSAGE_1_54TO58_DESCRIPTION")
	ELSE
		CALL KOJO_27_0_REGIST_DESCRIPTION_AFTER("KOJO_27_0_SYSTEM_MESSAGE_1_54TO58_DESCRIPTION")
	ENDIF
	
	; PTメンバーが居る場合の口上
	IF GET_MEMBER_SUM() > 1
		
		IF GET_MEMBER_SUM() == 2
			IF NO:1 == 27
				CALL KOJO_27_0_ADD_MESSAGE, @"「%KOJO_27_0_CALLNAME_FROM_ID(FLAG:10)%、"
			ELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"「%KOJO_27_0_CALLNAME_FROM_ID(1)%、"
			ENDIF
		ELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「みんな、"
		ENDIF
		CALL KOJO_27_0_PARAGRAPH_MESSAGE
		
		CALL KOJO_27_0_ADD_MESSAGE, @"祝福してぇ♥　あたしと%PETNAME%様と結婚するの♥\n　今からあたしたち、誓いのセックスするからぁ♥　見ててねぇっ♥♥」\n"
		
	; PTメンバーなしのソロプレイ口上
	ELSEIF GET_MEMBER_SUM() == 1
		CALL KOJO_27_0_ADD_MESSAGE, @"「あたしの全身に、%PETNAME%様を感じさせてください♥」\n"
	ENDIF
RETURN 1

