
; ------------------------------------------------------------------------------
; ■ 165 遭遇時会話：固定イベント情報取得　取得側口上
; ------------------------------------------------------------------------------

; □ 汎用口上
@KOJO_27_0_SYSTEM_MESSAGE_165(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	; 好感度によって分岐
	; 好感度:低
	IF CFLAG:(ARG:1):5 < 0
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"（%KOJO_27_0_CALLNAME_FROM_ID(ARG:1)%なんかの話が役に立つとは思えないけどね……）\n"
		ENDSELECT
		
	; 好感度:警戒
	ELSEIF CFLAG:(ARG:1):5 < 50
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"（ま、話半分に聞いときましょ……）\n"
		ENDSELECT
		
	; 好感度:通常
	ELSEIF CFLAG:(ARG:1):5 < 200
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"（些細なことでも覚えておけば何かしら役に立つでしょ……）\n"
			CALL KOJO_27_0_ADD_MESSAGE, @"「役に立つかどうかはともかく、覚えておいた方がいいわね」\n"
		ENDSELECT
		
	; 好感度:高
	ELSEIF CFLAG:(ARG:1):5 >= 200
		SELECTCASE KOJO_MODE
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「情報ありがと♪　%KOJO_27_0_CALLNAME_FROM_ID(ARG:1)%の話は役に立つわ～♪」\n"
		ENDSELECT
	ENDIF
	
	
	; 取得した情報によって分岐
	SELECTCASE ARG:2
	; キャラクタの捕食状況
	CASE 0 TO 99
		; 声を聞いただけ
		IF ARG:3 == 0
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"（部屋の外まで響く声って、戦いか……アレよねえ♥）\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"（もしかしたら、その近くに出口があるんじゃ……？）\n"
				CALL KOJO_27_0_ADD_MESSAGE, @"（敵か味方か判らないけど、近くに行ったら確認した方がいいかも）\n"
			ENDSELECT
			
		; 捕まってる
		ELSE
			SELECTCASE KOJO_MODE
			CASE "淫乱"
				CALL KOJO_27_0_ADD_MESSAGE, @"（へぇ……　早く助けないと、あたしみたいに頭バカにされちゃうかもね♥）\n"
			CASE "強気"
				CALL KOJO_27_0_ADD_MESSAGE, @"（気が向いたら助けてあげようかしらね、気が向いたら）\n"
			CASEELSE
				CALL KOJO_27_0_ADD_MESSAGE, @"（うう……　あんまり聞きたくない話だわ……）\n"
			ENDSELECT
		ENDIF
	
	; アイテム情報
	CASE 108, 109, 110, 111, 119, 169
		SELECTCASE KOJO_MODE
		CASE "発情"
			CALL KOJO_27_0_ADD_MESSAGE, @"「まともな道具が落ちてればいいけど……」\n"
		CASEELSE
			CALL KOJO_27_0_ADD_MESSAGE, @"「あたしが行くまで残ってるといいなー」\n"
		ENDSELECT
	ENDSELECT
RETURN 1


