
; ------------------------------------------------------------------------------
; ■ 14 敵の性攻撃 (命中) - 攻撃種類別 0691 ヴォジャノーイ 三段A
; ------------------------------------------------------------------------------

@KOJO_27_0_SYSTEM_MESSAGE_14_691(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
	
	; 追加地の文関数(前)を登録
	CALL KOJO_27_0_REGIST_DESCRIPTION_BEFORE("KOJO_27_0_SYSTEM_MESSAGE_14_DESCRIPTION_BEFORE")

	; 追加地の文関数(後)を登録
	CALL KOJO_27_0_REGIST_DESCRIPTION_AFTER("KOJO_27_0_SYSTEM_MESSAGE_14_DESCRIPTION_AFTER")
	
	; 睡眠・喪失・諦め
	SIF CFLAG:25 || CFLAG:29 || TFLAG:16 == 2
		RETURN 0
	
	; 催眠中
	IF KOJO_27_0_IS_HYPNO()
		; 催眠種類で分岐
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			
		; 発情・感情操作系
		CASE 1
			
		; 無意識・認識操作系
		CASE 2
			RETURN 0
		ENDSELECT
	ENDIF

	; 通常時
	; 拘束中の攻撃なので、それを意識した口上もあり
	SELECTCASE KOJO_MODE
	CASE "淫乱"
		CALL KOJO_27_0_ADD_MESSAGE, @"「お尻の穴、出たり入ったりぃっ……♥　んあぁぁっ♥　めくれちゃうぅぅっ♥♥」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「捕まって、お尻見えない状態でっ……　すごい、虐められてるぅぅっ♥♥」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（お尻メチャクチャにされてるけど……っ♥　捕まってて逃げられないから、仕方ないよねえ♥♥）\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（うそぉぉっ♥　これ全部あたしのお尻ほじくってる音なのぉぉっ♥♥）\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（舐め回されてる……♥　ヒドイ音でお尻の穴舐め回されてる……ッ♥♥）\n"
	CASEELSE
		CALL KOJO_27_0_ADD_MESSAGE, @"「んおぉ……っ！？　お尻の奧まで入って……っ！？　ひあぁぁっ、抜かれえぇッ！？　繰り返すなあぁっ！！」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"「あああぁっ！？　あたしのお尻に何してるのぉぉっ！？　怖いっ！　見えないの怖いよぉぉっ！！」\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（お尻メチャクチャにされてる……のにっ……　捕まってて逃げられないよぉっ……）\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（うそぉぉっ！？　これ全部あたしのお尻ほじくってる音なのぉぉっ！？）\n"
		CALL KOJO_27_0_ADD_MESSAGE, @"（舐め回されてる……！　ヒドイ音でお尻の穴舐め回されてる……ッ！！）\n"
	ENDSELECT
	CALL KOJO_27_0_PARAGRAPH_MESSAGE
	
	; 地の文側で\nするので口上側は\nで終わること
RETURN 1
