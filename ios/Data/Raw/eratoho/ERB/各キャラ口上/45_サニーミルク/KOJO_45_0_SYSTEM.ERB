;***********************************************************************
;		サニーミルク口上地の文
;***********************************************************************
;OP、EDだけ

@KOJO_45_0_SYSTEM(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

SELECTCASE ARG:0

	;記入済みのEDの地の文飛ばす
	CASE 204, 206
	;
	
	;OP
	CASE 203
		IF CHARANUM <= 2
			PRINTFORML 
			IF FLAG:AUTOMODE
				PRINTFORML 「それ」は毎月、少女を生贄として要求していた。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「それ」は毎月、少女を生贄として要求していた。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 生贄が捧げられなくなれば老若男女問わず襲い村ごと自らの栄養として取り込んだ。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 生贄が捧げられなくなれば老若男女問わず襲い村ごと自らの栄養として取り込んだ。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 「それ」は確かに強く、暴力的で、圧倒的なだったはずだ。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「それ」は確かに強く、暴力的で、圧倒的なだったはずだ。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML だが、「それ」はついに人と神と妖怪の手によって倒されてしまった…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW だが、「それ」はついに人と神と妖怪の手によって倒されてしまった…
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML それから100年？1000年？それともそれ以上だろうか？
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW それから100年？1000年？それともそれ以上だろうか？
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 「それ」が倒される間際に切り離した体の一部がかつてと変わらぬ力をついに取り戻したのだ。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「それ」が倒される間際に切り離した体の一部がかつてと変わらぬ力をついに取り戻したのだ。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 「それ」は早速、活動を再開した。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「それ」は早速、活動を再開した。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML しかし「それ」の身体は別の世界にいるかのように動物にも人間にも届くことはなかった…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW しかし「それ」の身体は別の世界にいるかのように動物にも人間にも届くことはなかった…
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML そして何も出来ないまま「それ」は絶望したまま静かに眠りについた…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW そして何も出来ないまま「それ」は絶望したまま静かに眠りについた…
			ENDIF
			PRINTFORML 
			IF FLAG:AUTOMODE
				PRINTFORML 「それ」が完全に人間に忘れ去られさらにどれほどの時が立ったのだろうか？
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「それ」が完全に人間に忘れ去られさらにどれほどの時が立ったのだろうか？
			ENDIF
			PRINTFORML 
			IF FLAG:AUTOMODE
				PRINTFORML やがて再び「それ」は蘇った。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW やがて再び「それ」は蘇った。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 今度は前回とは違い世界が自分に馴染む感覚に全身を震わせ大地から一本の触手を伸ばす。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 今度は前回とは違い世界が自分に馴染む感覚に全身を震わせ大地から一本の触手を伸ばす。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML それに驚いた野うさぎが逃げ出すのに気づいた「それ」はさらに数本の触手を生やすと野うさぎを捕らえ体内へと引きずり込んだ。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW それに驚いた野うさぎが逃げ出すのに気づいた「それ」はさらに数本の触手を生やすと野うさぎを捕らえ体内へと引きずり込んだ。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 久しぶりの肉の味に「それ」は興奮し、世界に触れられる喜びに涙すらした。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 久しぶりの肉の味に「それ」は興奮し、世界に触れられる喜びに涙すらした。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML だが物足りない。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW だが物足りない。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 「それ」が求める肉は単純なたんぱく質などではなかった。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「それ」が求める肉は単純なたんぱく質などではなかった。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 「それ」の中を必死に彷徨い、恐怖し、悲鳴をあげ、ささやかな無駄な抵抗を繰り返す少女が欲しかった。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「それ」の中を必死に彷徨い、恐怖し、悲鳴をあげ、ささやかな無駄な抵抗を繰り返す少女が欲しかった。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 心が折れるまで少女を陵辱し、じっくりと時間をかけて生命力を全て啜り上げる欲求にいてもたってもいられなかった。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 心が折れるまで少女を陵辱し、じっくりと時間をかけて生命力を全て啜り上げる欲求にいてもたってもいられなかった。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 興奮気味の「それ」の近くを一人の不幸な少女が元気よく駆けていく…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 興奮気味の「それ」の近くを一人の不幸な少女が元気よく駆けていく…
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 眩しいほどの笑顔で虫取り網をもって蝶を追い回す羽の生えたツインテールを赤いリボンで作った八重歯の少女。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 眩しいほどの笑顔で虫取り網をもって蝶を追い回す羽の生えたツインテールを赤いリボンで作った八重歯の少女。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 妖精のその少女を準備運動には丁度いい相手だと「それ」は認識すると、
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 妖精のその少女を準備運動には丁度いい相手だと「それ」は認識すると、
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 空高く舞い上がった蝶を恨めしそうに見上げる少女の背後へと触手を忍び寄らせたのだった…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 空高く舞い上がった蝶を恨めしそうに見上げる少女の背後へと触手を忍び寄らせたのだった…
			ENDIF
			PRINTFORML 

		ELSE
			PRINTFORML 
			IF FLAG:AUTOMODE
				PRINTFORML 神隠し異変
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 神隠し異変
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML そんな噂が人里で広まり始めてどの程度の時間がたったのだろうか？
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW そんな噂が人里で広まり始めてどの程度の時間がたったのだろうか？
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 少なくとも、人里の人間の殆どが人里の外に行きたがらなくなる程度の時間は過ぎていた。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 少なくとも、人里の人間の殆どが人里の外に行きたがらなくなる程度の時間は過ぎていた。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML そんな異変だ異変だと騒ぎになればお祭好きの妖精が黙っているわけもなく、
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW そんな異変だ異変だと騒ぎになればお祭好きの妖精が黙っているわけもなく、
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 光の三妖精も例に漏れず異変に関わろうと毎日あれこれと頭と体をフル回転させていた。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 光の三妖精も例に漏れず異変に関わろうと毎日あれこれと頭と体をフル回転させていた。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML そしてそんなある日、神隠しにあった人を探そすために三人は神隠しが多いという場所へと遊びに行ったのだった。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW そしてそんなある日、神隠しにあった人を探そすために三人は神隠しが多いという場所へと遊びに行ったのだった。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 誰が最初に神隠しにあった人を探せるか競争ね、とよーいドンで三人仲良く別の方向へと飛び立ってしばらくしての事…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 誰が最初に神隠しにあった人を探せるか競争ね、とよーいドンで三人仲良く別の方向へと飛び立ってしばらくしての事…
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML サニーミルクは獣道を歩きながら途方に暮れていた。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW サニーミルクは獣道を歩きながら途方に暮れていた。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML そもそもどうやって神隠しにあった人を見分けるのかとか
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW そもそもどうやって神隠しにあった人を見分けるのかとか
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML どう考えても言いだしっぺのスターが圧倒的に有利な気がするとか…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW どう考えても言いだしっぺのスターが圧倒的に有利な気がするとか…
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML とりあえず、考えていても仕方がない。
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW とりあえず、考えていても仕方がない。
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML 出会った人に片っ端から神隠しにあったか聞いていこうと結論をだしたサニー
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 出会った人に片っ端から神隠しにあったか聞いていこうと結論をだしたサニー
			ENDIF
			IF FLAG:AUTOMODE
				PRINTFORML その無防備な彼女の背中は忍び寄る気配に気づくはずもなかった…
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW その無防備な彼女の背中は忍び寄る気配に気づくはずもなかった…
			ENDIF
			PRINTFORML 
		ENDIF

	
	
	;その他は通常の地の文
	CASEELSE
	CALL SYSTEM_MESSAGE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)


ENDSELECT



