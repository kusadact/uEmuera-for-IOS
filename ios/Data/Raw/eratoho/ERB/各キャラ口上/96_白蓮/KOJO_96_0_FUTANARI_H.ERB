
;[FUTANARI_ADDITION.ERB] 睡眠時ふたなりレイプ・ふたなりセックス・アイテムねだり時の体要求
;ARG:2,ARG:3にそれぞれ（交わってる）相手,　自分自身　のキャラIDが入っている
;名前参照はそれぞれ　%CALLNAME:(ARG:2)%,　%CALLNAME:(ARG:3)%
;とは言ってもARG:3には必ず口上キャラ自身のID（= TARGET）が入るので%CALLNAME:(ARG:3)%は使わなくても問題ない
;LOCAL:6はレイプ・セックスイベント中の処女喪失で = 1　になるので、ふたなりレイプ開始時のCASE 0経由でCASE 4にきたか
;直接CASE 4に来たかの分岐に使える
;XX_XXを口上の番号に変えてご利用下さい。
@KOJO_96_0_FUTANARI(ARG:0,ARG:1,ARG:2,ARG:3,ARG:4)
SELECTCASE ARG:0
	;NPC時ふたなり化
	CASE 0
		SELECTCASE ARG:1
			;ARG:3がふたなり化
			CASE 0

		ENDSELECT
	;ふたなりレイプ開始時
	CASE 1
		SELECTCASE ARG:1
			;処女で犯される
			CASE 0
				IF FLAG:AUTOMODE
					PRINTFORML 「だ、だめ、だめです！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「だ、だめ、だめです！」
				ENDIF
				IF FLAG:AUTOMODE
					PRINTFORML 「わ、私、まだ誰ともっ、だめぇぇぇっ！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「わ、私、まだ誰ともっ、だめぇぇぇっ！」
				ENDIF

			;処女を犯す
			CASE 1
				IF FLAG:AUTOMODE
					PRINTFORML 「あなた、はじめてなんですよね……」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「あなた、はじめてなんですよね……」
				ENDIF
				IF FLAG:AUTOMODE
					PRINTFORML 「でも、ごめんなさい……。我慢、できないんですっ…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「でも、ごめんなさい……。我慢、できないんですっ…」
				ENDIF
			;なし
			CASE 2

			;なし
			CASE 3

			;非処女で犯されるor処女喪失後　CASE 0からここに来たりもする
			CASE 4
				;イベント中に処女喪失
				IF LOCAL:6 == 1
					PRINTFORML 「ま、まだ、するんですか！？」
					IF FLAG:AUTOMODE
						PRINTFORML 「さっき、私の始めてを、奪ったばかりなのにっ…！」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「さっき、私の始めてを、奪ったばかりなのにっ…！」
					ENDIF
				ELSE
					IF FLAG:AUTOMODE
						PRINTFORML 「だ、だめっ！」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「だ、だめっ！」
					ENDIF
					IF FLAG:AUTOMODE
						PRINTFORML 「お、落ち着いてください！」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「お、落ち着いてください！」
					ENDIF
				ENDIF
			;非処女を犯すor処女奪った後
			CASE 5
				;イベント中に処女奪った
				IF LOCAL:6 == 1
					PRINTFORML 「ふふっ、もうはじめては私が奪ったじゃないですか」
					PRINTFORM 「抵抗しても無駄ですよ？ またしてあげますからね
					CALL HEARTCB(0xD02830)
					IF FLAG:AUTOMODE
						PRINTFORML 」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 」
					ENDIF
				ELSE
					PRINTFORML 「ごめんなさい、がまんできなくて…」
					IF FLAG:AUTOMODE
						PRINTFORML 「ずぶずぶって、させてください…」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「ずぶずぶって、させてください…」
					ENDIF
				ENDIF

		ENDSELECT
	;ふたなりレイプ抵抗成功
	CASE 2
		SELECTCASE ARG:1
			;処女喪失後　抵抗成功時
			CASE 0
				IF FLAG:AUTOMODE
					PRINTFORML 「も、もうこれ以上はやめてください……」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「も、もうこれ以上はやめてください……」
				ENDIF
			;処女奪った後　抵抗成功される
			CASE 1
				PRINTFORML 
			;非処女で犯される　抵抗成功時
			CASE 2
				PRINTFORML 「だ、だめですからっ！」
				IF FLAG:AUTOMODE
					PRINTFORML 「落ち着いてください！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「落ち着いてください！」
				ENDIF

			;非処女を犯す　抵抗成功される
			CASE 3
				PRINTFORML 「……だめ、ですか？」
				IF FLAG:AUTOMODE
					PRINTFORML 「気持ちいいのに…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「気持ちいいのに…」
				ENDIF
			;処女喪失後　抵抗成功後
			CASE 4
				IF FLAG:AUTOMODE
					PRINTFORML 「あ、危なかった…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「あ、危なかった…」
				ENDIF
			;処女奪った後　諦める
			CASE 5
				PRINTFORML 「ふふっ、安心してください」
				PRINTFORM 「今度はいっぱい気持ちよくしてあげますから…
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;非処女で犯される　抵抗成功後
			CASE 6
				IF FLAG:AUTOMODE
					PRINTFORML 「だ、駄目ですよ、こんな…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「だ、駄目ですよ、こんな…」
				ENDIF
			;非処女を犯す　諦める
			CASE 7

		ENDSELECT
	;ふたなりレイプ抵抗失敗
	CASE 3
		SELECTCASE ARG:1

			;処女喪失後　抵抗失敗時
			CASE 0
				PRINTFORML 「っぐ……ぁ……！」
				IF FLAG:AUTOMODE
					PRINTFORML 「そ、そんな……」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「そ、そんな……」
				ENDIF
			;処女奪った後　抵抗失敗時
			CASE 1
				PRINTFORM 「ふふっ、ずぶずぶって、入っちゃいましたよ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　さ、動いちゃいますね
				CALL HEARTCB(0xD02830)
				PRINTFORM 　大丈夫、すぐに気持ちよくなれますから…
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;非処女で犯される　抵抗失敗時
			CASE 2
				PRINTFORML 「わ、分かりました…」
				IF FLAG:AUTOMODE
					PRINTFORML 「もう、暴れませんから、痛くしないでくださいね…？」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「もう、暴れませんから、痛くしないでくださいね…？」
				ENDIF
			;非処女を犯す　抵抗失敗時
			CASE 3
				PRINTFORM 「ほら、ぬるーって
				CALL HEARTCB(0xD02830)
				PRINTFORM 　ふふっ、気持ちいいんですね？ 腰、動いちゃってますよ
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				PRINTFORM 「そんなにおねだりしなくても…
				CALL HEARTCB(0xD02830)
				PRINTFORM 　すぐに、い～っぱいにゅぽにゅぽしてあげますね
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
		;犯される側（主人公）が触手服装備時
			;処女喪失後　抵抗失敗後　触手服を着ている
			CASE 4
				PRINTFORM 「ひああぁぁぁぁっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　とまっへっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　らめらめらめぇぇぇっ
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;処女奪った後　抵抗失敗後　触手服を弄る
			CASE 5
				PRINTFORM 「そんな悪い子は、おしおきですね
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				PRINTFORM 「ふふっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　そんなに気持ち良さそうに鳴いちゃって
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;非処女で犯される　抵抗失敗後　触手服を着ている
			CASE 6
				PRINTFORM 「ひっ！？　そ、それは、はあぁっ
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				PRINTFORM 「らめぇ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　うねうね、らめれしゅぅぅっ
				CALL HEARTCB(0xD02830, 2)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;非処女を犯す　抵抗失敗後　触手服を弄る
			CASE 7
				PRINTFORM 「ふふっ、抵抗しても無駄ですよ
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				PRINTFORM 「ほら、こうしたら……ふふっ、ほら、こんなすぐにとろとろ
				CALL HEARTCB(0xD02830, 2)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
		;犯される側（主人公）が触手服装備時その後
			;処女喪失後　抵(ry後　触手服を弄られたあと挿入れられる直前
			CASE 8
				PRINTFORM 「ひぅっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　ひぅぅぅ
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				PRINTFORM 「ら、め。なの、にぃ…
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;処女奪った後　抵(ry後　触手服を弄ったあと挿入れる直前
			CASE 9
				PRINTFORM 「ふふっ、もうとろとろですね
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				PRINTFORM 「ほら、また入っちゃいますよ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　…ふふっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　そんなに気持ち良さそうな顔、しちゃって
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;非処女で犯される　抵(ry後　触手服を弄られたあと挿入れられる直前
			CASE 10
				PRINTFORM 「はーっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　はーっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　ぁ…ぅ…ぁ…
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				IF FLAG:AUTOMODE
					PRINTFORMDL 触手服に何度もイかされ、息も絶え絶えになって抵抗すら出来なくなった%CALLNAME:(TARGET)%を改めて襲い始めた。
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMDW 触手服に何度もイかされ、息も絶え絶えになって抵抗すら出来なくなった%CALLNAME:(TARGET)%を改めて襲い始めた。
				ENDIF
			;非処女を犯す　抵(ry後　触手服を弄ったあと挿入れる直前
			CASE 11
				PRINTFORM 「ふふっ、何回もイっちゃいましたね
				CALL HEARTCB(0xD02830)
				PRINTFORM 　こんなにとろとろ…これならぬるって入っちゃいますね
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
				PRINTFORM 「ほら、ずぶずぶ入っちゃいますよ…
				CALL HEARTCB(0xD02830)
				PRINTFORM 　いっぱい感じて、いっぱいイってくださいね
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
		;犯される側（主人公）が触手服以外の服着用だとここ　服が破けた時の口上がこれの後に来るので注意
			;処女喪失後　抵抗失敗時　服剥かれる
			CASE 12
				IF FLAG:AUTOMODE
					PRINTFORML 「だ、だめ、脱がさないでぇっ！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「だ、だめ、脱がさないでぇっ！」
				ENDIF
			;処女奪った後　抵抗失敗時　服を剥く
			CASE 13
				PRINTFORM 「ふふっ
				CALL HEARTCB(0xD02830)
				PRINTFORM 　ほら、ぬぎぬぎしましょうね
				CALL HEARTCB(0xD02830)
				IF FLAG:AUTOMODE
					PRINTFORML 」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 」
				ENDIF
			;非処女で犯される　抵抗失敗時　服剥かれる
			CASE 14
				PRINTFORM 「だ、だめ…脱がしちゃ、だめぇ…
