			CALL KOJO_37_0_ENDING_4
			
			;穣子が迷宮にいないで脱出
			ELSEIF CFLAG:802 == 0
			CALL KOJO_37_0_ENDING_5
			
			;穣子を囮に脱出
			ELSEIF NO:(FLAG:10) == 38 || NO:(FLAG:11) == 38
			CALL KOJO_37_0_ENDING_6
			
			;その他
			ELSE
			CALL KOJO_37_0_ENDING_7
			
			ENDIF

			PRINTFORML 
			PRINTFORML 
		
		;ガーディアン撃破、渦発見
		CASE 206, 208
		
			PRINTFORML 
			PRINTFORML 
			;穣子と脱出
			IF NO:(FLAG:10) == 38 || NO:(FLAG:11) == 38 
			CALL KOJO_37_0_ENDING_10
			
			;妊娠末期又は寄生かつPTに穣子がいない
			ELSEIF CFLAG:22 || CFLAG:36 == 3
			CALL KOJO_37_0_ENDING_4

			;穣子がPTに居ない状態でガーディアン倒して脱出
			;置き去りの帰らぬ穣子心配エンド
			ELSEIF (CFLAG:802 == 2 || CFLAG:802 == 3) && ARG:0 == 206
			CALL KOJO_37_0_ENDING_8
			
			;穣子が迷宮にいなかった
			ELSEIF CFLAG:802 == 0
			CALL KOJO_37_0_ENDING_9
			
			;穣子が迷宮内に居ることをしらなかった
			;ほぼ不可能？
			ELSEIF (CFLAG:802 == 1 || CFLAG:802 == 6) && CFLAG:803
			CALL KOJO_37_0_ENDING_11

			
			;穣子生還(渦エンドで穣子がPT外で生存)、またはリタイア
			ELSE
			CALL KOJO_37_0_ENDING_12
				
			ENDIF
		PRINTFORML 
		PRINTFORML 
		
		;クイーンソロとかできねーよ！
		CASE 212
		;最初からボッチ
		IF CHARANUM <= 2
			CALL KOJO_37_0_ENDING_13
		ELSE
			CALL KOJO_37_0_ENDING_14
		ENDIF
		
		CASEELSE
			;汎用
	ENDSELECT
	ENDIF

ENDIF





CALL KOJO_37_0_TORAUMA(ARG:0, ARG:1, ARG:2, ARG:3, ARG:4)

RESETCOLOR

;口上記入済み
RETURN 1

