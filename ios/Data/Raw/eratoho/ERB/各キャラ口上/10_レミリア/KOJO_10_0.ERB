ELSEIF CFLAG:828
	;ただし地の文で自分から突っこんでるやつは除く
	SIF !(ARG:0 == 14 && ARG:1 == 964)
	CALL SYSTEM_MESSAGE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
;アグーの寄生攻撃命中
ELSEIF ARG:0 == 16 && ARG:1 == 859
;Cワンプ系のNP吸収で吸収した
ELSEIF ARG:0 == 17 && ARG:1 == 715
;ホフゴブリンの性攻撃のうちいくつかを1/2の確率で
ELSEIF CFLAG:810 == 0 && (RAND:2 == 0 && (((ARG:0 == 14 || ARG:0 ==4) && ARG:1 == 410 ) || (ARG:0 == 14 && ARG:1 == 719) ))
