	IF CFLAG:806 < 3 && TALENT:75
		CFLAG:806 = 3
	ELSEIF CFLAG:806 > 3 && TALENT:75
		CFLAG:806 = 6
	ENDIF
ENDIF
;B
IF CFLAG:807 <= 6
CFLAG:807 = ABL:7 + ABL:11
	IF CFLAG:807 < 3 && TALENT:76
		CFLAG:807 = 3
	ELSEIF CFLAG:807 > 3 && TALENT:76
		CFLAG:807 = 6
	ENDIF
ENDIF
;C
IF CFLAG:808 <= 6
CFLAG:808 = ABL:8 + ABL:11
	IF CFLAG:808 < 3 && TALENT:77
		CFLAG:808 = 3
	ELSEIF CFLAG:808 > 3 && TALENT:77
		CFLAG:808 = 6
	ENDIF
ENDIF
;V
IF CFLAG:809 <= 6
CFLAG:809 = ABL:9 + ABL:11
	IF CFLAG:809 < 3 && TALENT:78
		CFLAG:809 = 3
	ELSEIF CFLAG:809 > 3 && TALENT:78
