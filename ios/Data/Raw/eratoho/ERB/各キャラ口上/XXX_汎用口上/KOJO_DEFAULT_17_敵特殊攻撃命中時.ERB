@KOJO_DEFAULT_17_22(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)



;●睡眠
IF CFLAG:25 > 0

	CALL COMMON_KOJO_DEFAULT_SLEEP_TEST(1)

	RETURN 1
ENDIF


	
	; important属性付与
	; 敵のその他攻撃に汎用口上关于無いけれど、専用口上なので設定
	CALL SET_IMPORTANT_FLAG
	
	; この攻撃が命中した場合、必ず催眠中となる
	; 記述口上关于全て催眠中のものになるので注意
	SELECTCASE CFLAG:43
	; 催眠強度1
	CASE 1
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			CALL ADD_RAND_MESSAGE, @"「啊……这是……？　身体……不听使唤……」"
			CALL ADD_RAND_MESSAGE, @"「呜、动不了！？」"
			
		; 発情・感情操作系
		CASE 1
			CALL ADD_RAND_MESSAGE, @"（刚才为什么那么讨厌呢……明明只是给我带来舒服的事情……）"
			CALL ADD_RAND_MESSAGE, @"（嗯……？　为什么肚子里……这么热……？）"
			
		; 無意識・認識操作系
		CASE 2
			CALL ADD_RAND_MESSAGE, @"「你做了什么……头晕晕的……」"
		ENDSELECT

	; 催眠強度2
	CASE 2
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			CALL ADD_RAND_MESSAGE, @"（好了，别再这样了！）"
			
		; 発情・感情操作系
		CASE 1
			CALL ADD_RAND_MESSAGE, @"（想要做爱♥　想要在任何地方都被插入♥）"
			CALL ADD_RAND_MESSAGE, @"（啊♥　不行♥　子宫都掉下来了♥）"
			
		; 無意識・認識操作系
		CASE 2
			CALL ADD_RAND_MESSAGE, @"（要服务……　必须要服务……）"
		ENDSELECT
		
	; 催眠強度最大、又关于レッサーヒュプノの減衰あり催眠
	CASEELSE
		SELECTCASE FLAG:526
		; 拘束・肉体操作系
		CASE 0
			CALL ADD_RAND_MESSAGE, @"（完全被身体控制了……）"
			CALL ADD_RAND_MESSAGE, @"（身体……该怎么办……怎么办……！）"
			
		; 発情・感情操作系
		CASE 1
			CALL ADD_RAND_MESSAGE, @"「啊啊♥　啊啊啊啊♥　给我，给我快乐♥」"
			CALL ADD_RAND_MESSAGE, @"「嘿……♥　忍不住了……♥　强奸我吧，快点强奸我♥♥」"
			CALL ADD_RAND_MESSAGE, @"「子宫♥　宝宝的房间一直在颤抖♥♥　停不下来的♥♥」"
			
		; 無意識・認識操作系
		CASE 2
			CALL ADD_RAND_MESSAGE, @"「想要用哪里来感受快乐吗？\n　为了让大家感到快乐，%CALLNAME:TARGET%会做任何事情」"
			
			; 素質[尻穴狂い]所持時 or A感度3以上 追加口上
			IF TALENT:75 || ABL:6 >= 3
				CALL ADD_RAND_MESSAGE, @"「想要用哪里来感受快乐吗？\n　软绵绵的屁股洞是推荐哦」"
			ENDIF
			
			; 素質[淫壺]所持時 or V感度3以上 追加口上
			IF TALENT:78 || ABL:9 >= 3
				CALL ADD_RAND_MESSAGE, @"「想要用哪里来感受快乐吗？\n　已经改造成性交专用的滑溜溜的阴道，已经准备好了」"
			ENDIF
			
			; 素質[キス魔]所持時 or M感度3以上 追加口上
			IF TALENT:79 || ABL:15 >= 3
				CALL ADD_RAND_MESSAGE, @"「想要用哪里来感受快乐吗？\n　变态口交已经变成了喜欢被侵犯的变态嘴巴，尽情享受吧」"
			ENDIF
		ENDSELECT
	ENDSELECT
RETURN 1

