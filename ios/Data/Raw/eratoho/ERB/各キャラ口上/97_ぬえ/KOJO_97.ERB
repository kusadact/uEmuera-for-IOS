; ハートスプリット
; ハートの文字があったら色を付けて表示する
@SPLIT_HEARTL_K97(ARGS)
#DIMS	MESSAGE_ARRAY, 100
#DIM LCOUNT
	VARSET MESSAGE_ARRAY
	
	SPLIT ARGS, "♥", MESSAGE_ARRAY
	FOR LCOUNT,0, RESULT
		PRINTFORM %MESSAGE_ARRAY:(LCOUNT:0)%
		IF LCOUNT:0 != (RESULT - 1)
			CALL HEARTCB(0xD02830)
		ENDIF
	NEXT
	PRINTFORML
RETURN

