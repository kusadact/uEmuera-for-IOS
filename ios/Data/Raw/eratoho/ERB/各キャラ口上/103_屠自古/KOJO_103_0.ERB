				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「祟って…やるわ…」
				;(神子、布都と一緒。もっといい分岐方法はある。確実に)
				ELSEIF FLAG:10 == 104 || FLAG:11 == 104|| FLAG:15 == 104 || FLAG:16 == 104 || FLAG:10 == 105 || FLAG:11 == 105 || FLAG:15 == 105 || FLAG:16 == 105
					SELECTCASE RAND:4
						CASE 0
							PRINTFORML 「私におまかせを！」
						CASE 1
							PRINTFORML 「豪族の意地ってもんがあるのよ！！」
						CASEELSE
							PRINTFORML 「またなの？」
					ENDSELECT
				ELSE
					SELECTCASE RAND:10
						CASE 0
							PRINTFORML 「超電磁ヨーヨー！……ヨーヨーって何？」
						CASE 1
							IF FLAG:AUTOMODE
								PRINTFORML 「私の栗羊羹食べたって…どうして布都は確認しないのかしら？」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「私の栗羊羹食べたって…どうして布都は確認しないのかしら？」
							ENDIF
							IF FLAG:AUTOMODE
								PRINTFORML 「ばかー！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「ばかー！」
							ENDIF
							PRINTFORMDL どうやら小さな祟りを起こしているようだ…。
						CASE 2
							PRINTFORML 「そこッ！」
						CASE 3
							PRINTFORML 「今のはミナデインじゃないわ、デインよ！」
						CASEELSE
							PRINTFORML 「やってやんよ！」
					ENDSELECT
				ENDIF
			;中弾幕
			CASE 11
				;発情
				IF CFLAG:28
					PRINTFORML 「ガゴッ…はぁ、くッ……ガゴウジサイクロンッ！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 屠自古は息を荒げながらも弾幕を放った…。
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 屠自古は息を荒げながらも弾幕を放った…。
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「まだまだ……ここでやられちゃ一族に合わせる顔が無いもの」
				;神子、布都といっしょ
				ELSEIF FLAG:10 == 104 || FLAG:11 == 104|| FLAG:15 == 104 || FLAG:16 == 104 || FLAG:10 == 105 || FLAG:11 == 105 || FLAG:15 == 105 || FLAG:16 == 105
						SELECTCASE RAND:4
						CASE 0
							PRINTFORML 「轟雷に塵と消えなさい！！」
						CASE 1
							PRINTFORML 「下がって！ここは私に！」
