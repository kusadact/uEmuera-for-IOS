@BATTLE_MIKATA_KOUDOU_SONOTA_82_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)


	SELECTCASE ARG:1
		;防御
		CASE 1
			;身を守る
			IF ARG:2 == 0
				IF CFLAG:800 >= 5
					PRINTDATAL
						DATAFORM 「無理…っ、攻めきれない…！」
						DATAFORM 「少し…落ち着かないと…っ」
						DATAFORM 「やだ、もう…やめろってばぁ…！」
						DATAFORM 「来るなぁぁ…」
					ENDDATA
				;拘束中
				ELSEIF CFLAG:20
					PRINTDATAL
						DATAFORM 「ちょっとやそっとじゃ自由になれそうにないねぇ。　今は耐えるしかないか」
						DATAFORM 「下手に暴れたらなにされるか…ぞっとしないね」
						DATAFORM 「うぎぎ、歯がゆい」
					ENDDATA
				ELSE
				 	PRINTDATAL
				 		DATAFORM 「耐えろ！　耐えるんだあたい！」
				 		DATAFORM 「今は雌伏のとき！　やがて来る至福(しょうり)のときのために！」
						DATAFORM 「あたいのしなやかなボディがダメージを的確に軽減してくれる。　してくれるのだ！」
						DATAFORM 「防戦一方だとちょっとまずいなぁ」
						DATAFORM 「攻めにまわる余裕がない…」
					ENDDATA
				ENDIF
				;リロードフラグ洗浄
				SIF CFLAG:805 == 5
					CFLAG:805 = 0
			;リロード
			ELSEIF ARG:2 == 1
				;リロード二刀流
				IF CFLAG:805 == 5
					CFLAG:805 = 0
				;一回目
				ELSE
				
					CFLAG:805 = 5
				ENDIF

			ENDIF
		;アイテム
		CASE 2
		;前進
		CASE 3
		;後退
		CASE 4
		;逃走
		CASE 5
			;この逃走は成功する
			IF ARG:2 == 1
				;見捨てそう
				IF CFLAG:1:20 || CFLAG:(FLAG:10):20 || CFLAG:(FLAG:11):20
					PRINTDATAL
						DATAFORM 「…………っ」
						DATAFORM 「あう…う…」
						DATAFORM (本当にいいのか？　でも…でも…っ)
					ENDDATA
				ELSEIF CFLAG:800 >= 5
					PRINTDATAL
						DATAFORM 「この機を逃したらもう…！」
						DATAFORM 「これ以上は本当にまずい…っ」
						DATAFORM 「逃げるんだ…もたもたしてたら…っ」
					ENDDATA
				ELSE
					PRINTDATAL
						DATAFORM 「あー…あたいちょっと用事を思い出しまして…」
						DATAFORM 「今だダッシュ！」
						DATAFORM 「すすす……」
						DATAFORM 「！　今のうちならいける…？」
						DATAFORM 「これは潮時ってやつかね…」
					ENDDATA
				ENDIF
			;失敗する
			ELSE
				IF CFLAG:800 >= 5
					PRINTDATAL
						DATAFORM 「この足じゃすぐに追いつかれちまう…」
						DATAFORM 「ううう…もういーじゃんかよう…」
						DATAFORM 「はぁ…う……いまさら…逃がすつもりもない、ってかい…」
					ENDDATA
				ELSE
					PRINTDATAL
						DATAFORM 「あんまりしつこいと女の子に嫌われちまうよ！」
						DATAFORM 「やっぱり帰っちゃ…ダメ？」
						DATAFORM 「ナイスディフェンス！　とか言ってる場合じゃない！」
						DATAFORM 「にゃー…そう簡単に逃がしちゃくれないか…」
						DATAFORM 「なんだい、無料じゃ通してくれないってかい？」
					ENDDATA
				ENDIF
			ENDIF
	ENDSELECT
RETURN
@BATTLE_MIKATA_KOUDOU_DANMAKU_82_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

	SELECTCASE ARG:1
			;弾幕攻撃
			;弱弾幕
			CASE 10
				SELECTCASE RAND:5
				CASE 4
					PRINTFORML 「ねこぱんち弾幕！　実は結構痛いよ！」
				CASE 3
					PRINTFORML 「塵も積もれば粉塵爆発。　無駄にもならなきゃばかにもできない」
				CASE 2
					PRINTFORML 「ふふふー体力は確実に奪っていくものさー」
				CASE 1
					PRINTFORML 「しっかり狙ってしっかりばら撒く！」
				CASEELSE
					PRINTFORML 「ちゃんと当ててきゃいつかは倒れるよねぇ？」
				ENDSELECT
			;中弾幕
			CASE 11
				SELECTCASE RAND:6
				CASE 5
					PRINTFORML 「さあ！　そこをどきな！」
				CASE 4
					PRINTFORML 「車輪の先端から火が出るいめーじで作ってみましたっと！」
				CASE 3
					PRINTFORML 「さーて、大きく振りかぶってー…おりゃ！」
				CASE 2
					PRINTFORML 「拾う骨くらいは残ると思うよ」
				CASE 1
					PRINTFORML 「地獄の針山はこんなもんじゃなかったけどね。これでも十分強力さ」
				CASEELSE
					PRINTFORML 「ちと焦げ付くくらいは勘弁しておくれよ！」
				ENDSELECT
			;強弾幕
			CASE 12
				SELECTCASE RAND:6
				CASE 5
					PRINTFORML 「おりゃーっ！」
				CASE 4
					PRINTFORML 「満員御礼、怨霊弾けりゃ寒気と痛みが吹きすさぶ！」
				CASE 3
					PRINTFORML 「威力にこだわりすぎると制御難しくなるんでちと不安だけど…」
				CASE 2
					PRINTFORML 「危ないやつらにゃ危ない弾幕！　容赦なしだよ！」
				CASE 1
					PRINTFORML 「跳ねて回って隙を撃つ！」
				CASEELSE
					PRINTFORML 「釣りだす怨霊にあたいの知った顔がないことを祈るよ」
				ENDSELECT
			;溜め撃ち
			CASE 13
				;溜め
				IF CFLAG:31 == 2
					SELECTCASE RAND:4
					CASE 3
						PRINTFORML 「じゃじゃーん！　ここであたいの隠し玉！」
					CASE 2
						PRINTFORML 「いっちょ派手にいくとしますかねぇ」
					CASE 1
						PRINTFORML 「ちんたらやっててもしゃーないね。　一気に押しきるよ！」
					CASEELSE
						PRINTFORML 「そいじゃ一発力を込めて…」
					ENDSELECT
				;発射
				ELSE
					SELECTCASE RAND:5
					CASE 4
						PRINTFORML 「そこになおれ！　叩き潰してやる！」
					CASE 3
						PRINTFORML 「躰も魂もこんがり焼けな！」
					CASE 2
						PRINTFORML 「でっかい花火を地上で一発！」
					CASE 1
						PRINTFORML 「これが地獄の燐火さ！」
					CASEELSE
						PRINTFORML 「"妖怪『火焔の車輪』"！　燃え尽きなあ！」
					ENDSELECT
				ENDIF
			;ばら撒き弾
			CASE 14
				SELECTCASE RAND:6
				CASE 5
					PRINTFORML 「反撃の隙を与えるなー！」
				CASE 4
					PRINTFORML 「にゃーん」
				CASE 3
					PRINTFORML 「あんまちょろちょろされても困るんでねぇ」
				CASE 2
					PRINTFORML 「この先速度制限エリアでーす。　スピード違反は減点だよ！」
				CASE 1
					PRINTFORML 「こういう地道な努力と策であたいは生き延びてきたのさ！」
				CASEELSE
					PRINTFORML 「狩りの鉄則逃げ場を封じる作戦！」
				ENDSELECT
			;貫通弾
			CASE 15
				SELECTCASE RAND:5
				CASE 4
					PRINTFORML 「こいつで全員お陀仏になっちまいな！」
				CASE 3
					PRINTFORML 「ぶち抜く一発ってのはどうも不得手だなァ。　お空の方が得意だろうね」
				CASE 2
					PRINTFORML 「肉体をすり抜け魂だけ削り取る怨霊の通りはどうだい！」
				CASE 1
					PRINTFORML 「まとめて喰らえー！」
				CASEELSE
					PRINTFORML 「獲物に次々飛びつく飢えた魂の欠片。　こういう時にゃ便利だねぇ」
				ENDSELECT
			ENDSELECT
RETURN

