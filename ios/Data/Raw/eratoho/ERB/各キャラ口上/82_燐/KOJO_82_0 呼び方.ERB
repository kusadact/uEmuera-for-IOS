
;%CALLNAME_82_0(その時に対象のキャラ番号/IDを読み取れる関数,必要であれば補佐の数値)%で呼び出す。

@CALLNAME_82_0(ARG:0, ARG:1)
#FUNCTIONS
	IF CFLAG:(ARG:0):104 == 0
		SELECTCASE NO:(ARG:0)
		CASE 81
			RETURNF "さとり様"
		CASE 83
			RETURNF "お空"
		CASE 84
			RETURNF "こいし様"
		;勇儀
		CASE 80
			RETURNF "姐さん"
		;映姫
		CASE 36
			RETURNF "閻魔様"
		;上海蓬莱
		CASE 88,89
			RETURNF "お人形さん"
		;ナズーリン
		CASE 91
			RETURNF "ネズミちゃん"
		;芳香
		CASE 101
			RETURNF "死体のお姉さん"
		;あなた
		CASE 0
			RETURNF "お前さん"
		;独断でお姉さん呼びと判断　萃香は鬼だから一応お姉さん呼び。諏訪子は少し悩んだ結果下に。旧作は独断度あっぷ
		CASE 1,2,6 to 9,12,14,16 to 23,26,28 to 32,34,35,37 to 43,49 to 54,57,58,61 to 65,67 to 72,73 to 76,85 to 87,92 to 98,102 to 106,108 to 114,116
			RETURNF "お姉さん"
		;独断でお嬢ちゃん呼びと判断
		CASE 3 to 5,10,11,13,15,24,25,27,33,44 to 48,55,56,59,60,66,73,90,99,100,107,115
			RETURNF "お嬢ちゃん"
		;残りの地霊メンバーとか追加とか記入漏れとかは名前で
		CASEELSE
			RETURNF CALLNAME:(GET_ID_FROM_NO(NO:(ARG:0)))
		ENDSELECT
	;燐の娘系列
	ELSEIF NO:(ARG:0) == 82
		RETURNF "お前さん"
	;他の娘
	ELSE
		RETURNF "お嬢ちゃん"
	ENDIF
