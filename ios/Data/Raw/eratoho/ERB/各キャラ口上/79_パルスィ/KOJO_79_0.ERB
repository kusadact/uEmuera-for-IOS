;　東方触手宮　お試しパルスィ口上

;口上基礎知識
;　口上を書く時は基本的に ARG:0 と ARG:1 の中に「シチュエーション」が入っているので、
;　それに応じて分岐を行い、適切なメッセージを表示する……というのが基本方針になります
;　
;　なお、TARGET に口上を表示するキャラが指定された状態になっているので、
;　自分の名前や自分のCFLAGを見たい時などは CALLNAME:TARGET とか CFLAG:1 とかで参照できます
;

;口上選択時のタイトル
@KOJO_TITLE_79_0
RESULTS = 「東方触手宮用パルスィ口上」0618α版　By ぱ。


;口上選択時の詳細説明
;フリーフォーマットです。ご自由にどうぞ
@KOJO_DETAIL_79_0
PRINTFORML とりあえず作ってみただけです。
PRINTFORML 今後気が向いたら分岐を考えてみるかも？


;文字色　エメラルドグリーン（明るめ）
@KOJO_DEFAULT_COLOR_79_0
RETURN RGBCOLOR(150, 200, 150)

;口上本体
@KOJO_79_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

;地の文はデフォルトのものを使用します
CALL SYSTEM_MESSAGE(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

;文字色設定
SETCOLOR CFLAG:700

SELECTCASE ARG:0
	
	CASE 1
		SELECTCASE ARG:1
			;防御
			CASE 1
				;発情
				IF CFLAG:28
					PRINTFORML 「や……、だめ、今は……っ！」
					PRINTFORML 「身体、へんなの……今されたら、むり……！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは発情させられた体を抱くようにして身を守っている！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは発情させられた体を抱くようにして身を守っている！
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「やあぁっ、もう、もうやめて…っ！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは恐怖に震える身体を抱き、身体を丸めている！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは恐怖に震える身体を抱き、身体を丸めている！
					ENDIF
				;全裸
				ELSEIF !BASE:CP
					PRINTFORML 「ひっ、こら、寄るなっ！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは裸体を隠すように両腕で身を守っている！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは裸体を隠すように両腕で身を守っている！
					ENDIF
				;その他
				ELSE
					IF FLAG:AUTOMODE
						PRINTFORML 「ううっ、やめてよね…触るなあっ！」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「ううっ、やめてよね…触るなあっ！」
					ENDIF
					;捕まっていない仲間がいるか
					IF (NO:1 != 79 && !CFLAG:1:20) || (NO:(FLAG:10) != 79 && !CFLAG:(FLAG:10):20) || (NO:(FLAG:11) != 79 && !CFLAG:(FLAG:11):20)
						IF FLAG:AUTOMODE
							PRINTFORML 「ほっ、ほらぁ！　早く倒してよ！」
							TWAIT FLAG:AUTOMODE * 400, 0
						ELSE
							PRINTFORMW 「ほっ、ほらぁ！　早く倒してよ！」
						ENDIF
					ENDIF
				ENDIF
			;アイテム
			CASE 2
				IF FLAG:AUTOMODE
					PRINTFORML 「こんなときは、さっき拾ったアレで……！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「こんなときは、さっき拾ったアレで……！」
				ENDIF
			;前進
			CASE 3
				PRINTFORML 「ああもう、わかったわよ！　やればいいんでしょやれば！」
				IF FLAG:AUTOMODE
					PRINTFORMDL 　パルスィは触手を掻い潜るように前に出ると、緑の瞳で敵を睨む！
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMDW 　パルスィは触手を掻い潜るように前に出ると、緑の瞳で敵を睨む！
				ENDIF
			;後退
			CASE 4
				PRINTFORML 「ひっ、ちょ、やっぱ気持ち悪い！　ダメだわこれっ！」
				IF FLAG:AUTOMODE
					PRINTFORMDL 　パルスィは尻もちをつきそうになりながら後ずさった…
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMDW 　パルスィは尻もちをつきそうになりながら後ずさった…
				ENDIF
				;捕まっていない仲間がいるか
				IF (NO:1 != 79 && !CFLAG:1:20) || (NO:(FLAG:10) != 79 && !CFLAG:(FLAG:10):20) || (NO:(FLAG:11) != 79 && !CFLAG:(FLAG:11):20)
					IF FLAG:AUTOMODE
						PRINTFORML 「こ、今度はあんたの番！　倒してきなさい！」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「こ、今度はあんたの番！　倒してきなさい！」
					ENDIF
				ENDIF
			;逃走
			CASE 5
				;発情
				IF CFLAG:28
					PRINTFORML 「た、ったすけ……っ！　今ヤバいの…っ！　はやく、ああぁ」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィはふらつく腰に力をこめ、逃げ出そうとした！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィはふらつく腰に力をこめ、逃げ出そうとした！
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「いや…もう嫌よ、こんなところっ、やあああぁっ！！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは恐怖に駆られて逃走を試みた！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは恐怖に駆られて逃走を試みた！
					ENDIF
				;その他
				ELSE
					SELECTCASE RAND:2
						CASE 0
							IF FLAG:AUTOMODE
								PRINTFORML 「無理無理無理っ！　逃げるわよ！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「無理無理無理っ！　逃げるわよ！」
							ENDIF
						CASEELSE
							IF FLAG:AUTOMODE
								PRINTFORML 「こんな化け物の相手していられるもんですか！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「こんな化け物の相手していられるもんですか！」
							ENDIF
					ENDSELECT
				ENDIF
			
			;弾幕攻撃
			;弱弾幕
			CASE 10
				;発情
				IF CFLAG:28
					PRINTFORML 「やっ、ダメ……こないでっ！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは股間の疼きをこらえ、心細いながらも弾幕を放つ！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは股間の疼きをこらえ、心細いながらも弾幕を放つ！
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「どうせ……こんな、私なんかの弾幕では」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは形だけで妖力の籠らない弾幕を放った！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは形だけで妖力の籠らない弾幕を放った！
					ENDIF
				;その他
				ELSE
					SELECTCASE RAND:2
						CASE 0
							IF FLAG:AUTOMODE
								PRINTFORML 「小手調べ！　これでもくらいなさい！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「小手調べ！　これでもくらいなさい！」
							ENDIF
						CASEELSE
							IF FLAG:AUTOMODE
								PRINTFORML 「本当に効いてるの？　気味が悪いわ……」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「本当に効いてるの？　気味が悪いわ……」
							ENDIF
					ENDSELECT
				ENDIF
			;中弾幕
			CASE 11
				;発情
				IF CFLAG:28
					PRINTFORML 「今は……っ、あ、うぅ……こ、これでっ！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは弱弱しく腰を引きながらも、なんとか弾幕を織り上げた！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは弱弱しく腰を引きながらも、なんとか弾幕を織り上げた！
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「こんなもの……どうせ、当たらないんでしょうよ」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは涙目になりながらも得意の弾幕を放った！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは涙目になりながらも得意の弾幕を放った！
					ENDIF
				;その他
				ELSE
					SELECTCASE RAND:2
						CASE 0
							IF FLAG:AUTOMODE
								PRINTFORML 「お気楽そうで妬ましいわね。修正してあげるわ！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「お気楽そうで妬ましいわね。修正してあげるわ！」
							ENDIF
						CASEELSE
							IF FLAG:AUTOMODE
								PRINTFORML 「食らいなさい、恨符「丑の刻参り」！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「食らいなさい、恨符「丑の刻参り」！」
							ENDIF
					ENDSELECT
				ENDIF
			;強弾幕
			CASE 12
				;発情
				IF CFLAG:28
					PRINTFORML 「だっだめ、あっちに行きなさいっ！！」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは欲情に集中を乱され、ありったけの妖力を叩きつけた！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは欲情に集中を乱され、ありったけの妖力を叩きつけた！
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					PRINTFORML 「これが当たらなかったら……もう、だめ、かしら」
					IF FLAG:AUTOMODE
						PRINTFORMDL 　パルスィは朦朧とする意識で巨大な緑の弾を撃ち出した！
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMDW 　パルスィは朦朧とする意識で巨大な緑の弾を撃ち出した！
					ENDIF
				;その他
				ELSE
					SELECTCASE RAND:2
						CASE 0
							IF FLAG:AUTOMODE
								PRINTFORML 「終わりにしてあげる、そこを動くんじゃないわよ！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「終わりにしてあげる、そこを動くんじゃないわよ！」
							ENDIF
						CASEELSE
							IF FLAG:AUTOMODE
								PRINTFORML 「私の気持ちがわかる？　吹っ飛びなさい、嫉妬「ジェラシーボンバー」！」
								TWAIT FLAG:AUTOMODE * 400, 0
							ELSE
								PRINTFORMW 「私の気持ちがわかる？　吹っ飛びなさい、嫉妬「ジェラシーボンバー」！」
							ENDIF
					ENDSELECT
				ENDIF
			;溜め撃ち
			CASE 13
				;溜め
				IF CFLAG:31 == 2
					IF FLAG:AUTOMODE
						PRINTFORML 「いい子で待っていなさいね。この弾幕には準備が要るの……」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「いい子で待っていなさいね。この弾幕には準備が要るの……」
					ENDIF
				;発射
				ELSE
					IF FLAG:AUTOMODE
						PRINTFORML 「これで完成よ、食らいなさい！　恨符「丑の刻参り七日目」！」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「これで完成よ、食らいなさい！　恨符「丑の刻参り七日目」！」
					ENDIF
				ENDIF
			;ばら撒き弾
			CASE 14
				PRINTFORML 「ちょこまか動くのなら…」
				IF FLAG:AUTOMODE
					PRINTFORML 「これでどうかしら…、花咲爺「シロの灰」！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「これでどうかしら…、花咲爺「シロの灰」！」
				ENDIF
			;貫通弾
			CASE 15
				PRINTFORML 「見えない魔物はどこまでも追い詰めるわ…」
				IF FLAG:AUTOMODE
					PRINTFORML 「逃げ回りなさい、妬符「グリーンアイドモンスター」！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「逃げ回りなさい、妬符「グリーンアイドモンスター」！」
				ENDIF
			
			;消耗武器
			;弱攻撃
			CASE 20
				;発情
				IF CFLAG:28
					IF FLAG:AUTOMODE
						PRINTFORML 「や、ちょっ、寄るなっ！　あっ」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「や、ちょっ、寄るなっ！　あっ」
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					IF FLAG:AUTOMODE
						PRINTFORML 「こんな武器じゃ、とても……」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「こんな武器じゃ、とても……」
					ENDIF
				;その他
				ELSE
					IF FLAG:AUTOMODE
						PRINTFORML 「次は当てるだけじゃ済まないわよ！」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「次は当てるだけじゃ済まないわよ！」
					ENDIF
				ENDIF
			;中攻撃
			CASE 21
				;発情
				IF CFLAG:28
					IF FLAG:AUTOMODE
						PRINTFORML 「ひっいいい、だっだめ、だめええぇ」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「ひっいいい、だっだめ、だめええぇ」
					ENDIF
				;MP1/4以下
				ELSEIF MAXBASE:MP/4 >= BASE:MP
					IF FLAG:AUTOMODE
						PRINTFORML 「……こんな物に頼らなきゃいけないなんて」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「……こんな物に頼らなきゃいけないなんて」
					ENDIF
				;その他
				ELSE
					IF FLAG:AUTOMODE
						PRINTFORML 「ふん、弾が無くなるまで撃ち込んでやる」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「ふん、弾が無くなるまで撃ち込んでやる」
					ENDIF
				ENDIF
			;強攻撃
			CASE 22
				;発情
				IF CFLAG:28
					IF FLAG:AUTOMODE
						PRINTFORML 「やあぁ、腰が……だめっ、寄るなっ、来ると撃つわよっ！？」
						TWAIT FLAG:AUTOMODE * 400, 0
					ELSE
						PRINTFORMW 「やあぁ、腰が……だめっ、寄るなっ、来ると撃つわよっ！？」
