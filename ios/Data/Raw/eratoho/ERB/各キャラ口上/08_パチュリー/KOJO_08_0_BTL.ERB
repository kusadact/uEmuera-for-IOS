@BTL_SAVEATTACK_8_0()
;味方が捕らわれている相手を攻撃

	;攻撃対象が誰かを捕食・拘束・丸呑みしてる
	IF DA:(TFLAG:53):10
		LOCAL:0 = DA:(TFLAG:53):10;
	ELSEIF DA:(TFLAG:53):20
		LOCAL:0 = DA:(TFLAG:53):20;
	ELSEIF DA:(TFLAG:53):21
		LOCAL:0 = DA:(TFLAG:53):21;
	ELSE
		LOCAL:0 = 0;
	ENDIF

	IF LOCAL:0
		;自分自身
		IF (NO:(LOCAL:0) == NO) && (CFLAG:(LOCAL:0):104 == CFLAG:104)
			IF FLAG:AUTOMODE
				PRINTFORML 「私が一方的に捕まったままだと…思ってるっ？」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「私が一方的に捕まったままだと…思ってるっ？」
			ENDIF
		;発情&触手中毒LV3
		ELSEIF CFLAG:28 && ABL:11 >= 3
			SELECTCASE RAND:4
			CASE 0
