				PRINTFORMW 「いっ？！　いやっ！！　入って来ちゃ…ぁあっ！　あぁ……」
			ENDIF
		;無視
		CASE 5
			;主人公
			IF TARGET == 1
				IF FLAG:AUTOMODE
					PRINTFORML 「流石にあの色の水は…近づかない方が無難ね」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「流石にあの色の水は…近づかない方が無難ね」
				ENDIF
			ENDIF
		;ふたなり化
		CASE 6
			CALL KOJO_FUTAKA_8_0
		ENDSELECT
	;不思議な泉　飲むとHP・MP・EP全快　入ると寄生を含む全状態異常治療　処女モードの場合処女も回復
	CASE 135
