;イベント

@EVENT_RANDOM_8_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
;ランダムイベント

	SELECTCASE ARG:0
	;トラップ
	CASE 200
		CALL EVENT_RANDOM_TRAP_8_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
		
	;PT分断テレポート
	CASE 201
		SELECTCASE ARG:1
		;飛ばされる前
		CASE 0
			IF FLAG:AUTOMODE
				PRINTFORML 「なっなに…？　この光───────……」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「なっなに…？　この光───────……」
			ENDIF
		;飛ばされた後
		CASE 1
			IF FLAG:AUTOMODE
				PRINTFORML 「ここは…さっきまで居た場所と違うわね…」
				TWAIT FLAG:AUTOMODE * 400, 0
			ELSE
				PRINTFORMW 「ここは…さっきまで居た場所と違うわね…」
			ENDIF
			IF FLAG:10 && FLAG:11
				IF FLAG:AUTOMODE
					PRINTFORML 「%CALLNAME_8_0(FLAG:10)%！　%CALLNAME_8_0(FLAG:11)%！」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「%CALLNAME_8_0(FLAG:10)%！　%CALLNAME_8_0(FLAG:11)%！」
				ENDIF
				IF FLAG:AUTOMODE
					PRINTFORML 「…返事が無いわね。　二人とも別の場所に飛ばされたのかしら…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「…返事が無いわね。　二人とも別の場所に飛ばされたのかしら…」
				ENDIF
			ELSEIF FLAG:10
				IF FLAG:AUTOMODE
					PRINTFORML 「%CALLNAME_8_0(FLAG:10)%も見当たらない…どこか別の場所に飛ばされたのかしら…」
					TWAIT FLAG:AUTOMODE * 400, 0
				ELSE
					PRINTFORMW 「%CALLNAME_8_0(FLAG:10)%も見当たらない…どこか別の場所に飛ばされたのかしら…」
				ENDIF
			ENDIF
		ENDSELECT
		
	ENDSELECT

RETURN




;ランダムイベント
;-----------------------------------------------------------------------------------------------------------------------------------------
@EVENT_RANDOM_TRAP_8_0(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)
;トラップイベント発生
;ARG:1にイベントID　ARG:2で同じイベントの詳細分岐
;●特殊イベント系

	;常に呼ぶ行動（戦意喪失でも）
	SELECTCASE ARG:1
	;ペット結婚式
	CASE 179
