;戦闘中にアイテムを使って貰った
@SYSTEM_MESSAGE_36(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

SELECTCASE ARG:1
	CASE 0
IF FLAG:AUTOMODE
    PRINTFORML (SYSTEM_MESSAGE_36)アイテムのIDが不正です ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  (SYSTEM_MESSAGE_36)アイテムのIDが不正です ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
ENDIF

	CASE 50
		PRINTFORML %ARGS:0%的HP回复了！
	CASE 51
		PRINTFORML %ARGS:0%的HP回复了！
	CASE 52
		PRINTFORML %ARGS:0%的MP回复了！
	CASE 53
		PRINTFORML %ARGS:0%的MP回复了！
	CASE 54
		PRINTFORML %ARGS:0%换上了新衣服！
	CASE 55
		PRINTFORML %ARGS:0%身心都冷静了下来！
	CASE 56
		PRINTFORML %ARGS:0%的面颊绯红、精力充沛起来！
	CASE 57
		IF CFLAG:28
			PRINTFORML %ARGS:0%的身体状况除了发热的身体外都恢复正常了！
		ELSE
			PRINTFORML %ARGS:0%的身体恢复正常了！
		ENDIF
	CASE 58
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%的眼睛里恢复了光芒！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%的眼睛里恢复了光芒！
ENDIF

	CASE 59
		;メッセージ不要
	CASE 60
		;メッセージ不要
	CASE 61
		PRINTFORML %ARGS:0%的最大HP增加了300！
	CASE 62
		PRINTFORML %ARGS:0%的战斗素质增加了10！
	CASE 63
		PRINTFORML %ARGS:0%的速度增加了10！
	CASE 64
		;メッセージ不要
	CASE 65
		;メッセージ不要
	CASE 66
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%上完厕所了！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%上完厕所了！
ENDIF

	CASE 67
		IF CFLAG:501 == 100 && GET_BUST(TARGET) >= 3
			PRINTFORM 为了隐藏那淫乱的肥大的乳房
		ELSEIF CFLAG:501 == 100 && GET_BUST(TARGET) >= 1
			PRINTFORM 为了隐藏那丰满的胸部
		ENDIF
		PRINTFORML %ARGS:0%缠上了绷带！
	CASE 69
		IF CFLAG:506 > 40
			PRINTFORML 由于母乳而膨胀的%ARGS:0%的胸部慢慢地恢复了原状。
		ELSE
			PRINTFORML %ARGS:0%的胸部瘪了下去！
		ENDIF
	CASE 70
		PRINTFORML %ARGS:0%吃下了%ITEMNAME:70%……
		PRINTFORML 太难吃了！%ARGS:0%的兴奋度下降了！
		PRINTFORML 而且不知道使什么成分在作祟、%ARGS:0%的身体仿佛有火在烧！
	CASE 71
		PRINTFORML %ARGS:0%使用了%ITEMNAME:71%！
		PRINTFORML 房间被稍微净化了！
	CASE 72
		PRINTFORML %ARGS:0%窥视了一下%ITEMNAME:72%……
		PRINTFORML 迷宫内的情形一目了然了！
	CASE 73
		PRINTFORML %ARGS:0%的HP回复了！
	CASE 74
		PRINTFORML %ARGS:0%在股间贴上了符札！
	CASE 75
		PRINTFORML %ARGS:0%在肚子上贴上了符札！
	CASE 76
		PRINTFORML %ARGS:0%在胸部贴上了符札！
	CASE 77
		PRINTFORML %ARGS:0%喝下了排卵诱发剂！
	CASE 78
		PRINTFORML %ARGS:0%喝下了产卵促进剂！
	CASEELSE
IF FLAG:AUTOMODE
    PRINTFORML (SYSTEM_MESSAGE_36)项目ID不正确 ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  (SYSTEM_MESSAGE_36)アイテムのIDが不正です ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
ENDIF

ENDSELECT

