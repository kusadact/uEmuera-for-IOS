;捕食された後の描写
@SYSTEM_MESSAGE_152(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

IF TARGET == 1
	;主人公本人＝主観描写
	SELECTCASE ARG:1
		CASE 0
IF FLAG:AUTOMODE
    PRINTFORML (SYSTEM_MESSAGE_152)敌人的ID不正确 ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  (SYSTEM_MESSAGE_152)敌人的ID不正确 ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
ENDIF

		CASEELSE
			IF ARG:2
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%被%ARGS:1%完全吞噬了…
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%被%ARGS:1%完全吞噬了…
ENDIF

			ELSE
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%被%ARGS:1%吸干了…
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%被%ARGS:1%吸干了…
ENDIF

			ENDIF
	ENDSELECT
ELSE
	;主人公以外＝客観描写
	SELECTCASE ARG:1
		CASE 0
IF FLAG:AUTOMODE
    PRINTFORML (SYSTEM_MESSAGE_152)敌人的ID不正确 ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  (SYSTEM_MESSAGE_152)敌人的ID不正确 ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
ENDIF

		CASEELSE
			IF ARG:2
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%被%ARGS:1%完全吞噬了…
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%被%ARGS:1%完全吞噬了…
ENDIF

			ELSE
IF FLAG:AUTOMODE
    PRINTFORML %ARGS:0%被%ARGS:1%吸干了…
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  %ARGS:0%被%ARGS:1%吸干了…
ENDIF

			ENDIF
	ENDSELECT
ENDIF
