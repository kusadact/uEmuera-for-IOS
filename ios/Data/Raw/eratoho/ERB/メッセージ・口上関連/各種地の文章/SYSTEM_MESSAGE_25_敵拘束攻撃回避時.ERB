;拘束攻撃回避時の描写
@SYSTEM_MESSAGE_25(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)


SELECTCASE ARG:1
	CASE 0
IF FLAG:AUTOMODE
    PRINTFORML (SYSTEM_MESSAGE_25)コマンドIDが不正です ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  (SYSTEM_MESSAGE_25)コマンドIDが不正です ({ARG:1}, %ARGS:0%, %ARGS:1%, {ARG:2}, {ARG:3}, {ARG:4}, {ARG:5}, {ARG:6})
ENDIF

	CASE 540
		PRINTFORML 用%ARGS:0%的双峰尽情地磨蹭了脸颊的%ARGS:1%感到很满足、释放了%ARGS:0%！
	CASE 575
		PRINTFORML 触手上长出的锐爪擦过了%ARGS:0%的鼻尖！%ARGS:0%勉强避开了触手！
	CASEELSE
		PRINTFORML %ARGS:0%回避了攻击！
ENDSELECT



