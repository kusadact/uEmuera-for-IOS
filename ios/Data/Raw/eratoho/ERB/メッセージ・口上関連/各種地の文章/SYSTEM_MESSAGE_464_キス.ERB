;キス　軽い冗談から徐々に濃厚に
@SYSTEM_MESSAGE_464(ARG:0, ARG:1, ARGS:0, ARGS:1, ARG:2, ARG:3, ARG:4, ARG:5, ARG:6)

IF ARG:1 == 0
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%和%ARGS:1%开了个玩笑。
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%和%ARGS:1%开了个玩笑。
	ENDIF

ELSEIF ARG:1 == 2
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:1%轻吻了%ARGS:0%的脸颊！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:1%轻吻了%ARGS:0%的脸颊！
	ENDIF

ELSEIF ARG:1 == 4
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%和%ARGS:1%的嘴唇重合在一起！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%和%ARGS:1%的嘴唇重合在一起！
	ENDIF

ELSEIF ARG:1 == 6
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%和%ARGS:1%的嘴唇多次重叠在一起！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%和%ARGS:1%的嘴唇多次重叠在一起！
	ENDIF

ELSEIF ARG:1 == 8
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%冲动地拥抱住%ARGS:1%、含住对方的舌头吻了一下！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%冲动地拥抱住%ARGS:1%、含住对方的舌头吻了一下！
	ENDIF

ELSEIF ARG:1 == 10
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%和%ARGS:1%拥抱在一起、亲吻了好一会儿！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%和%ARGS:1%拥抱在一起、亲吻了好一会儿！
	ENDIF

ELSEIF ARG:1 == 12
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%和%ARGS:1%互相揉搓着胸部、沉溺在亲吻的快乐中！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%和%ARGS:1%互相揉搓着胸部、沉溺在亲吻的快乐中！
	ENDIF

ELSEIF ARG:1 == 14
	IF FLAG:AUTOMODE
	    PRINTFORML %ARGS:0%和%ARGS:1%互相吮吸着对方口内的唾液、并开始摩擦着胯下！
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  %ARGS:0%和%ARGS:1%互相吮吸着对方口内的唾液、并开始摩擦着胯下！
	ENDIF

ENDIF
