	TRYCCALLFORM KOJO_{NO:1}_{CFLAG:1:701}_FUTANARI(14,ARG:1*4+ABS(LOCAL:6-1)*2+24,ARG:0,1,0)
	CATCH
	ENDCATCH
	TRYCCALLFORM KOJO_{NO:(ARG:0)}_{CFLAG:(ARG:0):701}_FUTANARI(14,ARG:1*4+ABS(LOCAL:6-1)*2+37,1,ARG:0,0)
	CATCH
	ENDCATCH
	RETURN 2
ELSE
	GOTO INPUT_LOOP_0
ENDIF

RETURN 0



;妊娠処理
;ARG
;	:0	対象キャラID
;	:1	妊娠する確率
;	:2	妊娠する敵ID
@FUTANARI_NINSHIN(ARG:0,ARG:1,ARG:2)

	;************避妊パッチ********************	
	SIF CFLAG:(ARG:0):70
		ARG:1 /= CFLAG:(ARG:0):70 * 25
	;******************************************


IF RAND:100 < ARG:1 && CFLAG:(ARG:0):22 == 0 && TALENT:1:122 == 0

	CFLAG:(ARG:0):36 = 2
	SIF RAND:4 == 0
		CFLAG:(ARG:0):36 = 1
	CFLAG:(ARG:0):22 = ARG:2
	CFLAG:(ARG:0):23 = 8 + LIMIT(RAND:4 + RAND:4 + RAND:4 + RAND:4, 1, 10)
ENDIF
RETURN 0


;ふたなり伝染処理　レイプ時LOCAL:0 = 0　セックス時LOCAL:0 = 1
@FUTANARI_MOVE(ARG:0,ARG:1,LOCAL:0)
;コンフィグで全員ふたなりのみ
SIF FLAG:520 != 1
	RETURN 0
;伝染される側に生えていなくて伝染する側に生えている
SIF CFLAG:(ARG:0):42 <= 0 || TALENT:(ARG:0):121 || CFLAG:(ARG:1):42 > 0 || TALENT:(ARG:1):121 || RAND:100 >= FUTANARI_PERCENT(9)
	RETURN 0
IF FLAG:AUTOMODE
	PRINTFORML 沒想到%CALLNAME:(ARG:0)%的陰茎突然大幅脈動撲向了%CALLNAME:(ARG:1)%的陰蒂開始了同化！
	TWAIT FLAG:AUTOMODE * 400, 0
ELSE
	PRINTFORMW 沒想到%CALLNAME:(ARG:0)%的陰茎突然大幅脈動撲向了%CALLNAME:(ARG:1)%的陰蒂開始了同化！
ENDIF
TFLAG:63 = 2
LOCAL:1 = CFLAG:(ARG:0):42 * 1000 / 2
SIF LOCAL:1 <= 0
	LOCAL:1 = 1000
CALL DAMAGE_COMMON(ARG:1, 2, RAND:(LOCAL:1) + CFLAG:(ARG:0):42 * 1000, 100)
IF FLAG:AUTOMODE
	PRINTFORML %CALLNAME:(ARG:0)%的陰茎伝染給了%CALLNAME:(ARG:1)%…
	TWAIT FLAG:AUTOMODE * 400, 0
ELSE
	PRINTFORMW %CALLNAME:(ARG:0)%的陰茎伝染給了%CALLNAME:(ARG:1)%…
ENDIF
CFLAG:(ARG:1):42 = CFLAG:(ARG:0):42
CFLAG:(ARG:0):42 = 0
IF RESULT
	TRYCCALLFORM KOJO_{NO:(ARG:1)}_{CFLAG:(ARG:1):701}_FUTANARI(12,0+LOCAL:0*2,ARG:0,ARG:1,0)
	CATCH
	ENDCATCH
	TRYCCALLFORM KOJO_{NO:(ARG:0)}_{CFLAG:(ARG:0):701}_FUTANARI(12,1+LOCAL:0*2,ARG:1,ARG:0,0)
	CATCH
	ENDCATCH
ENDIF
RETURN 1
