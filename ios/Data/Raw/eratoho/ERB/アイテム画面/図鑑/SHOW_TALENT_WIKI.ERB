;素質一覧表示
@SHOW_TALENT_WIKI
#DIMS SLV,4	;LVにより名称の変わる素質用
;LOCAL
;0	ループ用
;1	表示回数
;2	表示する素質番号の範囲
;3	ヘルプを見る素質番号
;4	ループ用2
;5	初期素質持ちがいるかどうかの判定
;6	直前に選択した番号
;7	素質を見たかどうか
;10	行削除用カウント1(計算用)
;11	行削除用カウント2(計算用)
;12	行削除用カウント3(項目の改行数)
;13	行削除用カウント4(計算用)
;14	行削除用カウント5(計算用)
;15	行削除用カウント6(詳細の改行数)

	$START
	REDRAW 0
	LOCAL:10 = LINECOUNT

	DRAWLINE
	PRINTFORML 要查看哪個素質？
	IF LOCAL:2 == 0 || LOCAL:2 > 300
		LOCAL:2 = 100
	ENDIF
	FOR LOCAL:0, (LOCAL:2 - 100), LOCAL:2
		IF TALENTNAME:(LOCAL:0) != ""
			PRINTFORM [{LOCAL:0+1,3}] %TALENTNAME:(LOCAL:0),15,LEFT%
			LOCAL:1 += 1
			;3つ表示するごとに改行
			SIF (LOCAL:1 % 3) == 0 && LOCAL:1 > 0
				PRINTL  
		ENDIF
	NEXT
	LOCAL:1 = 0
	PRINTL
	PRINTL
	PRINTFORM [500] %"上翻",15,LEFT%
	PRINTFORML [600] %"下翻",15,LEFT%
	PRINTFORML [  0] 取消



	$INPUT_LOOP
	INPUT

	LOCAL:11 = LINECOUNT
	;項目表示の改行数を記録
	LOCAL:12 = LOCAL:11 - LOCAL:10

	IF RESULT == 0
		REDRAW 1
		LOCAL:6 = 0
		LOCAL:7 = 0
		RETURN
	ELSEIF RESULT == 500
		IF LOCAL:2 > 100
			LOCAL:2 -= 100
		ELSE
			LOCAL:2 = 300
		ENDIF
		CLEARLINE LOCAL:12
		GOTO START
	ELSEIF RESULT == 600
		LOCAL:2 += 100
		CLEARLINE LOCAL:12
		GOTO START
	ELSEIF TALENTNAME:(RESULT-1) != "" && RESULT > 0
		;行削除(初回)
		IF LOCAL:7 == 0
			CLEARLINE LOCAL:12
		;行削除(2回目～)
		ELSEIF LOCAL:7 == 1 && LOCAL:6 != RESULT
			CLEARLINE LOCAL:12 + LOCAL:15 + 3
		;連打のログ流れ防止
		ELSEIF LOCAL:6 == RESULT
			CLEARLINE 1
			GOTO INPUT_LOOP
		ENDIF
		LOCAL:6 = RESULT
		LOCAL:3 = RESULT-1
		DRAWLINE
		SETCOLOR 0x60C0FF
		PRINTFORML %"["+TALENTNAME:(RESULT-1)+"]",15,LEFT% 
		RESETCOLOR
		LOCAL:13 = LINECOUNT
		CALL SHOW_TALENT_EXPLAIN_WIKI(RESULT-1)
		PRINTL
		SETCOLOR 0x60C0FF
		PRINTFORML >> 初期取得者
		RESETCOLOR

		VARSET SLV
		IF LOCAL:3 == 100
			SLV:1 = ■ [嬌小体型]
			SLV:2 = ■ [小人体型]
		ELSEIF LOCAL:3 >= 101 && LOCAL:3 <= 106
			SELECTCASE LOCAL:3
			CASE 101
				LOCALS:0 = Ａ
			CASE 102
				LOCALS:0 = Ｂ
			CASE 103
				LOCALS:0 = Ｃ
			CASE 104
				LOCALS:0 = Ｖ
			CASE 105
				LOCALS:0 = 肌
			CASE 106
				LOCALS:0 = Ｍ
			ENDSELECT
			SLV:0 = ■ [%LOCALS:0%鈍感]      (被ダメージ -40％)
			SLV:1 = ■ [%LOCALS:0%敏感]      (被ダメージ +30％)
			SLV:2 = ■ [%LOCALS:0%過敏]      (被ダメージ +60％)
			SLV:3 = ■ [%LOCALS:0%異常過敏]  (被ダメージ+100％)
		ELSEIF LOCAL:3 == 160
			SLV:1 = ■ [浄化知識]
			SLV:2 = ■ [結界知識]
		ELSEIF LOCAL:3 == 161
			SLV:1 = ■ [体術習熟]
			SLV:2 = ■ [剣術習熟]
		ELSEIF LOCAL:3 == 164
			SLV:1 = ■ [波動感知]
			SLV:2 = ■ [術式知識]
		ELSEIF LOCAL:3 == 169
			SLV:1 = ■ [神気]
			SLV:2 = ■ [神の血]
		ELSEIF LOCAL:3 == 213
			SLV:1 = ■ [羽]
			SLV:2 = ■ [翼]
		ELSEIF LOCAL:3 == 215
			SLV:1 = ■ [尻尾]
			SLV:2 = ■ [大量尾巴]
		ENDIF
		
		IF GROUPMATCH(LOCAL:3,100,101,102,103,104,105,106,160,161,164,213,215)
			IF LOCAL:3 >= 101 && LOCAL:3 <= 106
				SETCOLOR 0x2070FF
				PRINTFORML %SLV:0%
				RESETCOLOR
				FOR LOCAL:4, 1, FLAG:2 + 1
					IF CSVTALENT(LOCAL:4,LOCAL:3) == -1
						PRINTFORM %CSVNAME(LOCAL:4,0)%　
						SIF LOCAL:5 == 0
							LOCAL:5 += 1
					ENDIF
					IF LOCAL:4 + 1 == FLAG:2
						IF LOCAL:5
							PRINTL
						ELSE
							SETCOLOR 0x333333
							PRINTFORML －－－
							RESETCOLOR
						ENDIF
					ENDIF
				NEXT
				LOCAL:5 = 0
				PRINTL
			ENDIF
			PRINTFORML %SLV:1%
			FOR LOCAL:4, 1, FLAG:2 + 1
				IF CSVTALENT(LOCAL:4,LOCAL:3) == 1
					PRINTFORM %CSVNAME(LOCAL:4,0)%　
					SIF LOCAL:5 == 0
						LOCAL:5 += 1
				ENDIF
				IF LOCAL:4 + 1 == FLAG:2
					IF LOCAL:5
						PRINTL
					ELSE
						SETCOLOR 0x333333
						PRINTFORML －－－
						RESETCOLOR
					ENDIF
				ENDIF
			NEXT
			LOCAL:5 = 0
			PRINTL
			SIF LOCAL:3 >= 101 && LOCAL:3 <= 106
				SETCOLOR 0xD022D0
			PRINTFORML %SLV:2%
			RESETCOLOR
			FOR LOCAL:4, 1, FLAG:2 + 1
				IF CSVTALENT(LOCAL:4,LOCAL:3) == 2
					PRINTFORM %CSVNAME(LOCAL:4,0)%　
					SIF LOCAL:5 == 0
						LOCAL:5 += 1
				ENDIF
				IF LOCAL:4 + 1 == FLAG:2
					IF LOCAL:5
						PRINTL
					ELSE
						SETCOLOR 0x333333
						PRINTFORML －－－
						RESETCOLOR
					ENDIF
				ENDIF
			NEXT
			LOCAL:5 = 0
			PRINTL
			IF LOCAL:3 >= 101 && LOCAL:3 <= 106
				SETCOLOR 0xA00000
				PRINTFORML %SLV:3%
				RESETCOLOR
				FOR LOCAL:4, 1, FLAG:2 + 1
					IF CSVTALENT(LOCAL:4,LOCAL:3) == 3
						PRINTFORM %CSVNAME(LOCAL:4,0)%　
						SIF LOCAL:5 == 0
							LOCAL:5 += 1
					ENDIF
					IF LOCAL:4 + 1 == FLAG:2
						IF LOCAL:5
							PRINTL
						ELSE
							SETCOLOR 0x333333
							PRINTFORML －－－
							RESETCOLOR
						ENDIF
					ENDIF
				NEXT
				LOCAL:5 = 0
			ENDIF
		ELSE
			FOR LOCAL:4, 1, FLAG:2 + 1
				IF EXISTCSV(LOCAL:4, 0) <= 0
					CONTINUE
				ELSEIF CSVTALENT(LOCAL:4,LOCAL:3)
					PRINTFORM %CSVNAME(LOCAL:4,0)%　
					SIF LOCAL:5 == 0
						LOCAL:5 += 1
				ENDIF
				IF LOCAL:4 + 1 == FLAG:2
					IF LOCAL:5
						PRINTL
					ELSE
						SETCOLOR 0x333333
						PRINTFORML －－－
						RESETCOLOR
					ENDIF
				ENDIF
			NEXT
			LOCAL:5 = 0
		ENDIF

		LOCAL:14 = LINECOUNT
		;説明表示の改行数を記録
		LOCAL:15 = LOCAL:14 - LOCAL:13
		LOCAL:5 = 0
		PRINTFORML  
		LOCAL:7 = 1
		GOTO START
	ELSE
		CLEARLINE 1
		REUSELASTLINE 入力が不正です！
		GOTO INPUT_LOOP
	ENDIF

