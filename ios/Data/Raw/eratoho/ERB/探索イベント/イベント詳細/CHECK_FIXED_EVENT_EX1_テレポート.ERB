;固定イベント
;RETURN	0:失敗した　1:成功した　2:以後のイベントスキップ
@CHECK_FIXED_EVENT_EX1

CALL ADD_ABL_EXP_PT(0, 500)

;危機回避と術式知識が高いと避けられる
IF (ABL:1:5 + TALENT:1:164 + RAND:2 > 2) || (FLAG:10 && ABL:(FLAG:10):5 + TALENT:(FLAG:10):164  + RAND:2 > 2) || (FLAG:11 && ABL:(FLAG:11):5 + TALENT:(FLAG:11):164 + RAND:2 > 2)
	PRINTFORML 找到了沒有起動的魔法陣
	IF TALENT:1:164 == 2 || (FLAG:10 && TALENT:(FLAG:10):164 == 2) || (FLAG:11 && TALENT:(FLAG:11):164 == 2)
		PRINTFORM 看起来好像是転送的魔法陣
	ENDIF
	PRINTFORML 要進入麼？
	CALL ASK_YN
	SIF RESULT == 1
		RETURN 2
ENDIF

IF FLAG:AUTOMODE
    PRINTFORML 突然強烈的光芒籠罩了%CALLNAME:1%%TACHI()%！
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  突然強烈的光芒籠罩了%CALLNAME:1%%TACHI()%！
ENDIF

CALL PRINT_MESSAGE(1, 201, 0, CALLNAME:1, "")
CALL PRINT_MESSAGE(FLAG:10, 201, 0, CALLNAME:1, "")
CALL PRINT_MESSAGE(FLAG:11, 201, 0, CALLNAME:1, "")

; テレポートと同時にPTメンバーが外れる
; FLAG:10から削除を行うとFLAG:11のキャラがFLAG:10にスライドしてくるのでFLAG:11から処理
CALL RANDOM_TELEPORT(1)
CALL RANDOM_TELEPORT(FLAG:11)
CALL RANDOM_TELEPORT(FLAG:10)

FLAG:5 = CFLAG:1:4
FLAG:(600 + FLAG:5) = 1

IF FLAG:AUTOMODE
    PRINTFORML 之前瞬間閉上了眼睛的%CALLNAME:1%小心翼翼的睜開了也能叫、完全都被転送到其他的部屋了…
    TWAIT FLAG:AUTOMODE * 400, 0
ELSE
    PRINTFORMW  之前瞬間閉上了眼睛的%CALLNAME:1%小心翼翼的睜開了也能叫、完全都被転送到其他的部屋了…
ENDIF

IF FLAG:10 && FLAG:11
	IF FLAG:AUTOMODE
	    PRINTFORML 沒看到直到剛才還在一起的%CALLNAME:(FLAG:10)%們的身影。　看到是転送到其他部屋了…
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  沒看到直到剛才還在一起的%CALLNAME:(FLAG:10)%們的身影。　看到是転送到其他部屋了…
	ENDIF

ELSEIF FLAG:10
	IF FLAG:AUTOMODE
	    PRINTFORML 沒看到直到剛才還在一起的%CALLNAME:(FLAG:10)%的身影。　看到是転送到其他部屋了…
	    TWAIT FLAG:AUTOMODE * 400, 0
	ELSE
	    PRINTFORMW  沒看到直到剛才還在一起的%CALLNAME:(FLAG:10)%的身影。　看到是転送到其他部屋了…
	ENDIF

ENDIF
CALL PRINT_MESSAGE(1, 201, 1, CALLNAME:1, "")

RETURN 2


@RANDOM_TELEPORT(ARG:0)

IF ARG:0 == 0
	RETURN
ENDIF

