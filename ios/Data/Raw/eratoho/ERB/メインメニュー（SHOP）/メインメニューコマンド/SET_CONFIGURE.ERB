;=================================================
;コンフィグメイン処理
;
;=================================================
@SET_CONFIGURE
#DIM LCOUNT,3	;ループ用

#DIM ACTIVE, 100	;項目の有効記憶用

#DIM LEVEL	;階層記憶用
#DIM LEVELNO, 10	;階層番号記憶用
#DIMS LEVELNAME, 10	;階層項目名記憶用

;コンフィグ内でゆっくり表示する必要は無いので常時REDRAW 0
REDRAW 0
WHILE 1
	DRAWLINEFORM -
	PRINTFORML 請選択要変更的内容
	PRINTFORML 

	;---階層表示---
	SETCOLOR 0x666666
	FOR LCOUNT:0, 0, LEVEL
		;インデント
		FOR LCOUNT:1, 0, LCOUNT:0
			PRINTFORM 　
		NEXT
		PRINTFORML [  ] %LEVELNAME:(LCOUNT:0)%
	NEXT
	RESETCOLOR
	
	;---階層用パス作成---
	LOCALS = %""%
	FOR LCOUNT:0, 0, LEVEL
		LOCALS = %LOCALS%{LEVELNO:(LCOUNT:0)}_
	NEXT
	;---項目表示---
	FOR LCOUNT:0, 1, 100
		;返り値初期化
		VARSET RESULTS, "", 0, 5

		TRYCCALLFORM SET_CONFIGURE_%LOCALS%{LCOUNT:0}(0)
			
			;無効な項目なら
			IF RESULTS:0 == ""
				ACTIVE:(LCOUNT:0) = 0
				CONTINUE
			ENDIF
			
			;選択が無効な項目なら
			IF RESULT:0 < 0 
				ACTIVE:(LCOUNT:0) = 0
				SETCOLOR 0x666666
			ELSE
				ACTIVE:(LCOUNT:0) = 1
			ENDIF
			
			;インデント
			FOR LCOUNT:1, 0, LEVEL
				PRINTFORM 　
			NEXT
			
			;ボタン
			IF ACTIVE:(LCOUNT:0) == 0
				PRINTFORM [  ] 
			ELSE
				PRINTFORM [{LCOUNT:0,2}] 
			ENDIF
			
			;項目名
			PRINTFORM %RESULTS:0, 34, LEFT%
			
			;現在の設定
			IF RESULTS:1 != ""
				PRINTFORM 現在的設定：%RESULTS:1%
			ENDIF
			PRINTL 
			
			;補足
			FOR LCOUNT:1, 2, 5
				IF RESULTS:(LCOUNT:1) != ""
					;インデント
					FOR LCOUNT:2, 0, LEVEL
						PRINTFORM 　
					NEXT
					PRINTFORML 　%RESULTS:(LCOUNT:1)%
				ENDIF
			NEXT
			
			RESETCOLOR
		CATCH
			ACTIVE:(LCOUNT:0) = 0
		ENDCATCH
	NEXT
	PRINTL 
	;インデント
	FOR LCOUNT:0, 0, LEVEL
		PRINTFORM 　
	NEXT
	;戻るボタン
	IF LEVEL > 0
		PRINTFORML [ 0] 返回
	ELSE
		PRINTFORML [ 0] 完了
	ENDIF
	;入力
	$INPUT_LOOP
	INPUT
	
	;戻る
	IF RESULT == 0
		IF LEVEL > 0
			LEVEL --
		ELSE
			BREAK
		ENDIF
	;選択が有効な項目なら
	ELSEIF RESULT >= 1 && RESULT <= 99 && ACTIVE:(RESULT) == 1
		LOCAL = RESULT
		TRYCALLFORM SET_CONFIGURE_%LOCALS%{LOCAL}(1)
		;下の階層へ
		IF RESULT > 0
			TRYCALLFORM SET_CONFIGURE_%LOCALS%{LOCAL}(0)
			LEVELNO:(LEVEL) = LOCAL
			LEVELNAME:(LEVEL) = %RESULTS:0%
			LEVEL ++
		;トップへ
		ELSEIF RESULT < 0
			LEVEL = 0
		ENDIF
	ELSE
		PRINTFORML 輸入値不正確。請重新輸入
		GOTO INPUT_LOOP
	ENDIF
WEND
REDRAW 1

;=================================================
;コンフィグ項目
;ARG:0 処理ID（0:項目名表示 1:選択されたとき）
;
;(ARG:0が0の時)
;RESULT:0 表示パターン（0:項目の選択有効　-1:項目の選択無効）
;RESULTS:0 項目名(空の場合は項目非表示)
;RESULTS:1 現在の設定
;RESULTS:2～4 補足
;
;(ARG:0が1の時)
;RESULT:0 階層移動（0:移動無し 1:１つ下へ移動 -1:トップに戻る）
;
;=================================================
;コンフィグ項目
;「表示に関する設定」
;=================================================
@SET_CONFIGURE_1(ARG)

	;---項目の表示---
	IF ARG == 0
		RESULTS:0 = 与顕示有関的設定

	;---選択時の処理---
	ELSEIF ARG == 1
		RETURN 1
	ENDIF

