;=================================================
;コンフィグ項目
;「閲覧済みフラグのリセット」
;=================================================
@SET_CONFIGURE_1_1_1(ARG)

	;---項目の表示---
	IF ARG == 0
		RESULTS:0 = 已閲標記的復位
		
	;---選択時の処理---
	ELSEIF ARG == 1
		PRINTFORML 将已閲標記全部復位
		PRINTFORML 可以再次確認内容。
		PRINTFORML 要実行麼？
		PRINTFORML [1] 実行
		PRINTFORML 
		PRINTFORML [0] 取消
		$INPUT_LOOP
		INPUT
		
		IF RESULT == 0
			PRINTFORML 已経取消
		ELSEIF RESULT == 1
			FOR LOCAL:0, 400, 500
				FLAG:(LOCAL:0) &= 2
			NEXT
			PRINTFORMW 已閲標記已経復位了
		ELSE
			GOTO INPUT_LOOP
		ENDIF
	ENDIF
;=================================================
;コンフィグ項目
;「チュートリアルの表示」
;=================================================
@SET_CONFIGURE_1_1_2(ARG)

	;---項目の表示---
	IF ARG == 0
		RESULTS:0 = 教程的顕示
		IF FLAG:500 == 1
			RESULTS:1 = 顕示
		ELSE
			RESULTS:1 = 不顕示
		ENDIF
		
	;---選択時の処理---
	ELSEIF ARG == 1
		PRINTFORML 切換教程的顕示・非顕示
		PRINTFORML [1] 不顕示(默認)
		PRINTFORML [2] 顕示
		PRINTFORML 
		PRINTFORML [0] 取消
		$INPUT_LOOP
		INPUT
		IF RESULT == 0
			PRINTFORML 已経取消
		ELSEIF RESULT == 1
			FLAG:500 = 0
			FOR LOCAL:0, 400, 500
				FLAG:(LOCAL:0) |= 2
			NEXT
			PRINTFORMW 教程已経設定為不顕示了
		ELSEIF RESULT == 2
			FLAG:500 = 1
			FOR LOCAL:0, 400, 500
				FLAG:(LOCAL:0) &= 1
			NEXT
			PRINTFORMW 教程已経設定為顕示了
		ELSE
			GOTO INPUT_LOOP
		ENDIF
	ENDIF
