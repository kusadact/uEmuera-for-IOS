	ELSEIF ARG == 1
		PRINTFORML 是否使用複乳补丁？
		PRINTFORML 
		PRINTFORML [1] OFF(默认)
		PRINTFORML [2] ON
		PRINTFORML 
		PRINTFORML [0] 取消
		$INPUT_LOOP
		INPUT
		IF RESULT == 0
			PRINTFORML 已经取消
		;1～2
		ELSEIF RESULT >= 1 && RESULT <= 2
			FLAG:598 = RESULT - 1
			PRINTFORMW 设定完成
		ELSE
			GOTO INPUT_LOOP
		ENDIF
	ENDIF

;=================================================
;コンフィグ項目
;「ゲームの難易度」
;=================================================
@SET_CONFIGURE_96(ARG)

	;★難易度追加パッチ改変
	IF FLAG:999
		;---項目の表示---
		IF ARG == 0
			RESULTS:0 = 遊戲的難易度
			IF FLAG:4 == 1
				RESULTS:1 = Extra
			ELSE
				RESULTS:1 = Phantasm
			ENDIF
		;---選択時の処理---
		ELSEIF ARG == 1
			PRINTFORML 請設定遊戲的難易度
			PRINTFORML [1] Extra　 　以Normal為基準開始時処于消耗状態
			PRINTFORML [2] Phantasm　以Lunatic為基準開始時処于消耗状態
			PRINTFORML 
			PRINTFORML [0] 取消
			$INPUT_LOOPEX
			INPUT
			IF RESULT == 0
				PRINTFORML 已経取消
			ELSEIF RESULT == 1
				FLAG:4 = 1
				FLAG:999 = 1
				PRINTFORMW 設定完成
			ELSEIF RESULT == 2
				FLAG:4 = 3
				FLAG:999 = 2
				PRINTFORMW 設定完成
			ELSE
				GOTO INPUT_LOOPEX
			ENDIF
		ENDIF
	;★ここまで
	ELSE
		;---項目の表示---
		IF ARG == 0
			RESULTS:0 = 遊戲的難易度
			IF FLAG:4 == -1
				RESULTS:1 = Very easy
			ELSEIF FLAG:4 == 0
				RESULTS:1 = Easy
			ELSEIF FLAG:4 == 1
				RESULTS:1 = Normal
			ELSEIF FLAG:4 == 2
				RESULTS:1 = Hard
			ELSE
				RESULTS:1 = Lunatic
			ENDIF
			
		;---選択時の処理---
		ELSEIF ARG == 1
			IF FLAG:4 >= 0
				PRINTFORML 請設定遊戲的難易度
				PRINTFORML [1] Easy     想要欺負触手的人用
				PRINTFORML [2] Normal   想要有来有回的人用
				PRINTFORML [3] Hard     喜歡艱難遊戲的人用
				PRINTFORML [4] Lunatic  想一直全滅的人用
				PRINTFORML 
				PRINTFORML [0] 取消
				$INPUT_LOOP
				INPUT
				IF RESULT == 0
					PRINTFORML 已経取消
				;1～4
				ELSEIF RESULT >= 1 && RESULT <= 4
					FLAG:4 = RESULT - 1
					PRINTFORMW 設定完成
				ELSE
					GOTO INPUT_LOOP
				ENDIF
			ELSE
				PRINTFORMW Very easy的難易度無法変更
			ENDIF
		ENDIF
	ENDIF

;=================================================
;コンフィグ項目
;「現在の設定をデフォルトとして保存」
;=================================================
@SET_CONFIGURE_97(ARG)

	;---項目の表示---
	IF ARG == 0
		RESULTS:0 = 保存現状用於下周目引継

	;---選択時の処理---
	ELSEIF ARG == 1
		LOCAL:0 = 0
		PRINTFORML 可以把図鑑和配置文件設定引継到下一周目
		PRINTFORML 数據保存為 gloval.sav 文件
		PRINTFORML 要進行保存麼？
		PRINTFORML 
		PRINTFORML [1] 全部保存
		PRINTFORML [2] 保存配置文件設定
		PRINTFORML [3] 保存図鑑
		PRINTFORML 
		PRINTFORML [0] 取消
		$INPUT_LOOP
		INPUT
		IF RESULT == 0
			PRINTFORML 已経取消
		ELSEIF RESULT == 1
			FOR LOCAL:0, 0, 101
				GLOBAL:(LOCAL:0) = FLAG:(LOCAL:0 + 500)
			NEXT
			;敵と出会ったかどうか記録　図鑑用
			CALL SAVE_GLOBAL_ENEMY_WIKI_DATA()
			SAVEGLOBAL
			PRINTFORMW 保存完畢
		ELSEIF RESULT == 2
			FOR LOCAL:0, 0, 101
				GLOBAL:(LOCAL:0) = FLAG:(LOCAL:0 + 500)
			NEXT
			SAVEGLOBAL
			PRINTFORMW 保存完畢
		ELSEIF RESULT == 3
			;敵と出会ったかどうか記録　図鑑用
			CALL SAVE_GLOBAL_ENEMY_WIKI_DATA()
			SAVEGLOBAL
			PRINTFORMW 保存完畢
		ELSE
			GOTO INPUT_LOOP
		ENDIF
	ENDIF

