;=================================================
;コンフィグ項目
;「敵の名前」
;=================================================
@SET_CONFIGURE_1_7(ARG)
#DIM LCOUNT
#DIM SUPPORT_DATA

	VARSET SUPPORT_DATA
	
	;---項目の表示---
	IF ARG == 0
		RESULTS:0 = 敵人外表的設定
		IF FLAG:513 == 1
			RESULTS:1 = 不顕示
		ELSE
			RESULTS:1 = 顕示
		ENDIF
		
	;---選択時の処理---
	ELSEIF ARG == 1
		PRINTFORML 敵人的名字後面会顕示種族名
		PRINTFORML 選択（）部分是否顕示
		PRINTFORML [1] 顕示(默認)
		PRINTFORML [2] 不顕示
		PRINTFORML 
		PRINTFORML [0] 取消
		$INPUT_LOOP
		INPUT
		IF RESULT == 0
			PRINTFORML 已経取消
		ELSEIF RESULT == 1 || RESULT == 2
			FLAG:513 = RESULT - 1
		ELSE
			GOTO INPUT_LOOP
		ENDIF
		
		;名前を再設定
		FOR LCOUNT, 0, FLAG:1
			;敵によって必要な補助値が違うので設定
			SELECTCASE DA:LCOUNT:0
			;生物工廠 捕食キャラID
			CASE 4
				SUPPORT_DATA = DA:LCOUNT:87
			;娘 親のID
			CASE 19
				SUPPORT_DATA = DA:LCOUNT:46
			;奇美拉 親のID
			CASE 36
				SUPPORT_DATA = DA:LCOUNT:47
			CASEELSE
				;悪堕ち・簡易悪堕ちエネミーの場合は元キャラのID
				IF DA:LCOUNT:99>0
					SUPPORT_DATA = DA:LCOUNT:46
				ENDIF

			ENDSELECT
			TRYCCALLFORM SET_ENEMY_DATA_{DA:LCOUNT:0}(FLAG:1, SUPPORT_DATA)
				SAVESTR:LCOUNT = %SAVESTR:(FLAG:1)%
			CATCH
			ENDCATCH
		NEXT
	ENDIF

