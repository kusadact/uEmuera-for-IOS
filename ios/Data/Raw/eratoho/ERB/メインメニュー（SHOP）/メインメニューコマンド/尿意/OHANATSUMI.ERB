
;=================================================
;お花摘み可能かどうかチェック
;RESULT:0 判定結果(0:可能 -1:不可)
;=================================================
@CHECK_OHANATSUMI(処理ID = 0)
#DIM LCOUNT,3	;ループ用
#DIM 処理ID
	;0:判定だけ
	;1:理由メッセージも表示
#DIM PTID,3
#DIM PTSUM

#DIM 敵フラグ
#DIM 結界部屋フラグ
#DIM 尿意フラグ
#DIM 全員睡眠フラグ
#DIM 全員動けずフラグ
#DIM スピーチキャラ
	
	SIF FLAG:522 == 2
		RETURN -1
	
	;PTID更新
	VARSET PTID
	PTSUM = GET_MEMBER_SUM()
	FOR LCOUNT,0 , PTSUM
		PTID:(LCOUNT) = GET_MEMBER(LCOUNT)
	NEXT

	;===チェック===
	
	;現在地が結界部屋
	IF FLAG:5 == FLAG:98
		結界部屋フラグ = 1
	ELSE
		結界部屋フラグ = 0
	ENDIF
	

	;敵チェック
	敵フラグ = 0
	FOR LCOUNT, 1, FLAG:1
		;現在位置が被ってる敵
		IF FLAG:5 == DA:(LCOUNT):4
			
			;死体は除外
			SIF DA:(LCOUNT):1 <= 0
				CONTINUE
				
			;擬態は除外
			SIF DA:(LCOUNT):0 == 18 || DA:(LCOUNT):45 == 0
				CONTINUE
			
			敵フラグ = 1
			
			BREAK
		ENDIF
	NEXT

	;PTチェック
	尿意フラグ = 0
	全員睡眠フラグ = 1
	全員動けずフラグ = 1
	スピーチキャラ = 0
	FOR LCOUNT, 0, 3
		IF BASE:(PTID:(LCOUNT)):NP > 0
			尿意フラグ = 1
		ENDIF
		
		IF CFLAG:(PTID:(LCOUNT)):25 == 0
			全員睡眠フラグ = 0
		ENDIF
		
		IF CFLAG:(PTID:(LCOUNT)):25 == 0 || CFLAG:(PTID:(LCOUNT)):29 == 0
			全員動けずフラグ = 0
			IF スピーチキャラ == 0
				スピーチキャラ = LCOUNT
			ENDIF
		ENDIF
		
	NEXT

	;===判定===
	
	IF 全員睡眠フラグ == 1
		IF 処理ID == 1
			PRINTFORMW %CALLNAME:(PTID:0)%\@ PTID:1 > 0 ? 們 # \@夢見了去上廁所的事…
		ENDIF
		RETURN -1
	ELSEIF 全員動けずフラグ == 1
		IF 処理ID == 1
			PRINTFORMW %CALLNAME:(PTID:0)%\@ PTID:1 > 0 ? 們 # \@沒有任何動作…
		ENDIF
		RETURN -1
	ELSEIF 尿意フラグ == 0
		IF 処理ID == 1
			PRINTFORMW %CALLNAME:(スピーチキャラ)%提議去上廁所但是誰都沒有尿意…
		ENDIF
		RETURN -1
	ELSEIF 結界部屋フラグ == 0 && 敵フラグ == 1
		IF 処理ID == 1
			PRINTFORMW 同一部屋有敵人存在的状態下不能做那樣的事情！
		ENDIF
		RETURN -1
		
	ENDIF
RETURN 1
;≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡
;お花摘み処理
;≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡
@OHANATSUMI
#DIM LCOUNT,3	;ループ用
#DIM PTID,3
#DIM PTSUM
#DIM PTトイレ済みフラグ
#DIM おもらしメンバ
#DIM UNKおもらしメンバ
#DIM キャラID
#DIM FLAG観測者
#DIM FLAG襲撃
#DIMS UNK_おもらしチェック

;-MEMO-



	;-----PTID更新-----
	VARSET PTID
	PTSUM = GET_MEMBER_SUM()
	FOR LCOUNT,0, PTSUM
		PTID:(LCOUNT) = GET_MEMBER(LCOUNT)
	NEXT
	
	;-----観測者がいるかチェック-----
	FLAG観測者 = 0
	FOR LCOUNT, 1, FLAG:1
		;現在位置が被ってる敵
		IF FLAG:5 == DA:(LCOUNT):4
			
			;死体は除外
			SIF DA:(LCOUNT):1 <= 0
				CONTINUE
				
			;観測者以外は除外
			SIF DA:(LCOUNT):0 != 97
				CONTINUE
			
			FLAG観測者 = 1
			BREAK
		ENDIF
	NEXT
	
	;-----漏らしそうなメンバを抽出-----
	おもらしメンバ = 0
	FOR LCOUNT, 0, PTSUM
		IF PTID:(LCOUNT) > 0
			キャラID = PTID:(LCOUNT)
			IF BASE:(キャラID):NP > 60
				おもらしメンバ |= POWER(2, LCOUNT)
				;整理中の為一時無効化
				;CALL PRINT_MESSAGE(キャラID, 506, 6, CALLNAME:(キャラID), "",1)
			ELSE
				;整理中の為一時無効化
				;CALL PRINT_MESSAGE(キャラID, 506, 6, CALLNAME:(キャラID), "",0)
			ENDIF
		ENDIF
	NEXT
	;便意処理
	UNKおもらしメンバ = 0
	FOR LCOUNT, 0, PTSUM
		IF PTID:(LCOUNT) > 0
			キャラID = PTID:(LCOUNT)
			IF UNK_おもらしチェック(キャラID)
				UNKおもらしメンバ |= POWER(2, LCOUNT)
			ELSE
			ENDIF
		ENDIF
	NEXT
	おもらしメンバ |= UNKおもらしメンバ
	
	
	;＝＝＝お花摘み前イベント「観測者の視線（伏線）」＝＝＝
	IF FLAG観測者 == 1
		
		PRINTFORMW %CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@好像感覚到了什麼東西的視線
		PRINTL [0] 直接折回
		PRINTL [1] 当做錯覚
		PRINTL
		
		$INPUT_w0
		INPUT
		IF RESULT == 0
			PRINTFORMW 有不好預感的%CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@直接返回了。
			;お花摘み中断
			RETURN -1
			
		ELSEIF RESULT == 1
			PRINTFORMW %CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@認為那只是錯覚。
			
		ELSE
			CLEARLINE 1
			REUSELASTLINE ※無効な入力です！※
			GOTO INPUT_w0
		ENDIF


	;＝＝＝お花摘み前イベント「漏らす」＝＝＝
	ELSEIF おもらしメンバ || UNKおもらしメンバ
		PRINTFORMW 在尋找着合適地点的途中%GET_NYOU_名前(おもらしメンバ)%突然当場蹲下了！
		PRINTFORMW 衣服上的汚漬和脚下拡散的水窪説明了発生了什麼、看来是已経来不及了…
		
		;-----おもらし処理----
		LOCAL = 0	;はいてない取得メンバ
		FOR LCOUNT, 0, PTSUM
			IF PTID:(LCOUNT) > 0
				IF おもらしメンバ & POWER(2, LCOUNT)
					BASE:(PTID:(LCOUNT)):NP = 0
					EXP:(PTID:(LCOUNT)):失禁経験 += 1
					CALL CHANGE_TP(PTID:(LCOUNT), 21)
					CALL CHECK_GET_OMORASHI(PTID:(LCOUNT))
					
					CALL SET_はいてない(PTID:(LCOUNT))
					;触手服メンバは除外
					IF RESULT == 1 && CFLAG:(PTID:(LCOUNT)):40 == 0
						LOCAL |= POWER(2, LCOUNT)
					ENDIF
					
				ENDIF
			ENDIF
		NEXT
		WAIT
		
		
		
		PRINTFORM %GET_NYOU_名前(おもらしメンバ)%満臉通紅的做着善後工作
		IF LOCAL 
			PRINTFORML 、
			PRINTFORMW %GET_NYOU_名前(LOCAL)%脱掉了完全湿透的無法再接着穿下去的内衣…
		ELSE
			PRINTFORMW た。
		ENDIF
		
		;-----便意-----
		;便意ありだと漏れろ 処理が面倒なので強制的に漏れろ
		IF CFLAG:(キャラID):49
			CALL UNK_OMORASHI(キャラID, 100, 0, 0, 0)
		ENDIF
		
		;-----メッセージ-----
		FOR LCOUNT, 0, PTSUM
			IF PTID:(LCOUNT) > 0
				キャラID = PTID:(LCOUNT)
				IF LOCAL & POWER(2, LCOUNT)
					;整理中の為一時無効化
					;CALL PRINT_MESSAGE(キャラID, 506, 7, CALLNAME:(キャラID), "",1)
				ELSE
					;整理中の為一時無効化
					;CALL PRINT_MESSAGE(キャラID, 506, 7, CALLNAME:(キャラID), "",0)
				ENDIF
			ENDIF
		NEXT
		
		;-----尿意の残っているキャラをチェック-----
		LOCAL = 0
		FOR LCOUNT, 0, PTSUM
			IF PTID:(LCOUNT) > 0
				IF BASE:(PTID:(LCOUNT)):NP > 0
					LOCAL = 1
					BREAK
				ENDIF
			ENDIF
		NEXT
		
		;-----全員尿意が無ければお花摘み終了-----
		IF LOCAL == 0
			;トイレを済ませた。(意味深)
			PRINTFORMW %CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@上完了廁所。
			RETURN 1
		ENDIF
		
	ENDIF
	;＝＝＝お花摘み前イベントここまで＝＝＝

	;---襲撃判定---
	FLAG襲撃 = 0
	IF FLAG:(CFLAG:1:4 + 50) < 40
	ELSEIF FLAG:(CFLAG:1:4 + 50) < 70
		IF RAND:4 == 0
			FLAG襲撃 = 1
		ENDIF
	ELSEIF FLAG:(CFLAG:1:4 + 50) < 100
		IF RAND:3 == 0
			FLAG襲撃 = 1
		ENDIF
	ELSE
		IF RAND:2 == 0
			FLAG襲撃 = 1
		ENDIF
	ENDIF

	
	PRINTFORMDW 找到了合適地方的%CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@迅速的上完了廁所…
	;お花摘み画像表示用
	CALL PRINT_MESSAGE(PTID:0, 506, 99, CALLNAME:(PTID:0), "", 0)
	PRINTFORMDW ……
	IF FLAG観測者 == 1
		;＝＝＝観測者有り＝＝＝

		
		;-----尿意・便意クリア-----
		FOR LCOUNT, 0, PTSUM
			IF PTID:(LCOUNT) > 0
				BASE:(PTID:(LCOUNT)):NP = 0
				CFLAG:(PTID:(LCOUNT)):49 = 0
			ENDIF
		NEXT
		
		
		
		IF FLAG襲撃
			CALL OHANATSUMI_SHUGEKI2
		ELSE


			;PT全体の気配察知合計が3以上なら見つける
			LOCAL = 0
			FOR LCOUNT, 0, PTSUM
				IF PTID:(LCOUNT) > 0
					LOCAL += ABL:(PTID:(LCOUNT)):1
				ENDIF
			NEXT

			IF LOCAL >= 3
				PRINTFORML 部屋的角落小声響起水声的時候、%CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@周囲的墻壁突然睜開了眼睛
				PRINTFORMW 注意到了緊貼在墻上的観測者正凝視着這辺！
				PRINTL
				;水の種類
				IF FLAG:522 == 1
					;黄金水
					PRINTFORML 観測者的四周大屏的顕示出了從各種各樣的角度記録
					PRINTFORMW 下来的%CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@的排泄行為的実況映像、
					PRINTL
					PRINTFORML 意識到排泄行為"被看見了"的%CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@
					PRINTFORML 正慌慌張張地打算站起来的時候
					PRINTFORMW 從秘部排出的液体量増加了、導致無法中断這個行為…
					PRINTL
					PRINTFORML 結果%CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@直到最後也無法停下放尿演出
					PRINTFORMW 等到整理姿勢的時候撮影者已経不知道逃到哪裡去了…
					
				ELSEIF FLAG:522 == 0
					;愛液
					PRINTFORML 正在這時從秘部流出的体液量増加了所以無法動作……
					PRINTFORMW 這個行為一直被看到了最後然後在整理姿勢的時候偸窺者已経不知道逃到哪裡去了……
					PRINTL
				ENDIF
			ELSE
				PRINTFORML 部屋的角落小声響起水声的時、想要過度短暫的安息時間的%CALLNAME:PTID%\@ PTSUM > 1 ? 們 # \@
				PRINTFORMW 總感覚從部屋的某処傳来的視線、反而変得更累了……
				PRINTL
			ENDIF


			;効果はTP・MPダウン
			FOR LCOUNT, 0, PTSUM
				IF PTID:(LCOUNT) > 0
					IF TALENT:(PTID:(LCOUNT)):36
						;不知羞恥ならダメージなし
						LOCAL = 0
					ELSEIF TALENT:(PTID:(LCOUNT)):35
						;害羞なら２倍
						LOCAL = MAXBASE:(PTID:(LCOUNT)):MP * (5 + RAND:11) / 1000
					ELSE
						LOCAL = MAXBASE:(PTID:(LCOUNT)):MP * (3 + RAND:5) / 1000
					ENDIF
					BASE:(PTID:(LCOUNT)):MP = MAX(1, BASE:(PTID:(LCOUNT)):MP - LOCAL)
					BASE:(PTID:(LCOUNT)):TP = MAX(0, BASE:(PTID:(LCOUNT)):TP - LOCAL)
					CALL PRINT_MESSAGE(PTID:(LCOUNT), 506, 2000, CALLNAME:(PTID:(LCOUNT)), "",0)
				ENDIF
			NEXT
			
			
			
		ENDIF
		
		
	ELSE
		;＝＝＝通常＝＝＝
		
		
		
		;-----尿意・便意クリア-----
		FOR LCOUNT, 0, PTSUM
			IF PTID:(LCOUNT) > 0
				BASE:(PTID:(LCOUNT)):NP = 0
				CFLAG:(PTID:(LCOUNT)):49 = 0
			ENDIF
		NEXT
		
		
		
		IF FLAG襲撃
			CALL OHANATSUMI_SHUGEKI
		ELSE
			PRINTFORMDW スッキリした！
		ENDIF
		
		
	ENDIF


;=================================================
